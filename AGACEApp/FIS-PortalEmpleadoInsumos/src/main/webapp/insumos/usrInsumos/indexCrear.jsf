                                </h:outputText>
                            </p:column>
                            <p:column>
                                <h:outputText value="#{insumo.fecetInsumo.fechaFinPeriodo}">
                                    <f:convertDateTime pattern="dd/MM/yyyy" />
                                </h:outputText>
                            </p:column>
