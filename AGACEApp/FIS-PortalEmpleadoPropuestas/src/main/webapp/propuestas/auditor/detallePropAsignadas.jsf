<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:ui="http://java.sun.com/jsf/facelets">
<h:head>
<meta http-equiv="X-UA-Compatible" content="IE=Edge"/>
	<title>AGACE</title>
	<style type="text/css">
.ui-datatable .ui-column-filter {
	width: 40px;
}
</style>
<script type="text/javascript">
  var esEnviado = false;
  
  function recargaPagina() {
	  location.reload();
  }
  
  function checkSubmit() {
	  if (!esEnviado) {
		  esEnviado = true;
		  return true;
	  } else {
		  return false;
	  }
  }
 </script>
</h:head>
<h:body>
	<h:form id="formPropuestas" enctype="multipart/form-data;charset=UTF-8"
		styleClass="panel-principal" onkeypress="if (event.keyCode == 13) {
                                                 return false;
                                             }">
		<f:metadata>
		    <f:event type="preRenderView"
				listener="#{consultarPropuestasAsignadasManagedBean.cargaDatos}" />
		</f:metadata>
		<p:ajaxStatus onstart="PF('statusDialog').show();"
			onsuccess="PF('statusDialog').hide();" />
		<p:dialog modal="true" widgetVar="statusDialog"
			header="#{msj['titulo.panel.espere']}" draggable="false"
			closable="false" resizable="false">
			<p:graphicImage value="/resources/images/ajaxloadingbar.gif" />
		</p:dialog>
		<p:messages id="msgExitoGuardar" showDetail="true" globalOnly="true"
			closable="true" redisplay="false"/>

		<p:panel id="panelPropuestas" styleClass="panel-interno" 
			header="Propuestas #{consultarPropuestasAsignadasManagedBean.cpaStringHelper.encabezadoPropuestas}"
			rendered="#{consultarPropuestasAsignadasManagedBean.cpaBooleanHelper.muestraPropuestas}"
			>

			<!-- Grid de Propuestas  -->
			<div style="width: 750px; overflow-x: scroll;">
			<h:panelGrid id="panelPropuestasAnalizar" width="100%"
				rendered="#{consultarPropuestasAsignadasManagedBean.cpaBooleanHelper.muestraPropuestas}">
				<ui:include src="/propuestas/auditor/panelPropuestasAsignadas.jsf"></ui:include>
			</h:panelGrid>
			</div>
		</p:panel>
	</h:form>

	<!-- Detalle de la Propuesta-->
	<h:form id="formPropuesta" enctype="multipart/form-data;charset=UTF-8"
		styleClass="panel-principal" >
		<p:dialog modal="true" widgetVar="statusDialog"
			header="#{msj['titulo.panel.espere']}" draggable="false"
			closable="false" resizable="false">
			<p:graphicImage value="/resources/images/ajaxloadingbar.gif" />
		</p:dialog>

		<p:panel id="panelPropuesta" styleClass="panel-interno"
			header="#{msj['titulo.panel.detalle.propuesta.asignada']}"
			rendered="#{consultarPropuestasAsignadasManagedBean.cpaBooleanHelper.muestraDetalleContribuyente}">
			<p:outputLabel value="#{msj['separador.panel.datos.contribuyente']}"
				style="font-weight:bold;" />
			<p:separator />
			<p:spacer width="60" height="5px;" />

			<ui:include src="/propuestas/auditor/datosPropuesta.jsf" />

			<p:fieldset id="tolgateAcciones" widgetVar="tolgateAcciones" legend="Historial de acciones de la propuesta"
				toggleable="true" toggleSpeed="500" collapsed="true">
				<p:ajax event="toggle"
					listener="#{consultarPropuestasAsignadasManagedBean.handleToggleAcciones}"
					update="tablaHistorialAcciones" />


				<p:dataTable id="tablaHistorialAcciones" var="accion"
					value="#{consultarPropuestasAsignadasManagedBean.consultarPropAsignadaHelper.listaHistoricoAccion}"
					editable="false" emptyMessage="#{msj['label.lista.vacia']}"
					paginator="true" rows="5"
					paginatorTemplate="#{msj['btn.pagina']}: {CurrentPageReport} {FirstPageLink} {PreviousPageLink}   #{msj['btn.ir.a.pagina']}: {JumpToPageDropdown}  {NextPageLink} {LastPageLink}"
					currentPageReportTemplate="({currentPage} de {totalPages})"
					paginatorPosition="bottom">

					<f:facet name="header">#{msj['label.titulo.historial.acciones']}</f:facet>

					<p:column headerText="#{msj['column.fecha.hora.carga.documento']}">
						<div align="center">
							<h:outputText value="#{accion.fechaHora}">
								<f:convertDateTime pattern="dd/MM/yyyy HH:mm:ss" />
							</h:outputText>
						</div>
					</p:column>

					<p:column headerText="#{msj['label.observaciones.asignada']}">
						<div align="center">
							<h:outputText value="#{accion.observaciones}" />
						</div>
					</p:column>

					<p:column headerText="#{msj['label.causa.historial']}">
						<div align="center">
							<h:outputText value="#{accion.fececMotivo.descripcion}" />
						</div>
					</p:column>

					<p:column headerText="#{msj['label.accion.historial']}">
						<div align="center">
							<h:outputText
								value="#{accion.fececAccionesFuncionario.descripcion}" />
						</div>
					</p:column>

					<p:column headerText="#{msj['label.usuario.accion.historial']}">
						<h:outputText
							value="#{accion.empleadoDto.nombreCompleto} (  #{accion.descripcionTipoEmpleado} )" />

					</p:column>

					<p:column headerText="#{msj['label.documento']}">
						<div align="center">
							<p:commandLink value="#{accion.numeroDocumentos}" ajax="true"
								action="#{consultarPropuestasAsignadasManagedBean.obtenerDocumentosHistoricoAccion}"
								immediate="true"
								update=":formPropuesta:pnlDocumentoHistoricoAccion"
								style="color:#0170b7;"
								rendered="#{accion.numeroDocumentos != -1}">
								<f:setPropertyActionListener value="#{accion}" immediate="true"
									target="#{consultarPropuestasAsignadasManagedBean.consultarPropAsignadaHelper.historicoAccion}" />
							</p:commandLink>

							<h:outputText value="N/A"
								rendered="#{accion.numeroDocumentos == -1}" />

						</div>

					</p:column>

				</p:dataTable>

			</p:fieldset>
			<br />
			<br />
			<p:fieldset id="tolgateRetro" widgetVar="tolgateRetro" legend="#{msj['btn.historico.retroalimentacion']}"
				toggleable="true" toggleSpeed="500" collapsed="true">
				<p:ajax event="toggle"
					listener="#{consultarPropuestasAsignadasManagedBean.handleToggleRetrolimentacion}"
					update="tablaHistoricoRetroalimentacion" />

				<p:panel id="pnlHistoricoRetro" header="#{msj['titulo.historico']}" >
					<h:panelGrid columns="2" width="100%">
						<p:spacer width="530" height="1" />
						<h:outputText
							value="#{msj['label.registro']}: #{consultarPropuestasAsignadasManagedBean.cpaDtoHelper.propuestaSelAnalizar.idRegistro}"
							style="font-weight:bold;" />
					</h:panelGrid>


					<p:dataTable id="tablaHistoricoRetroalimentacion"
						var="retroalimentacion"
						value="#{consultarPropuestasAsignadasManagedBean.consultarPropAsignadaHelper.listaHistoricoRetroalimentar}"
						editable="false" emptyMessage="#{msj['label.lista.vacia']}"
						paginator="true" rows="5"
						paginatorTemplate="#{msj['btn.pagina']}: {CurrentPageReport} {FirstPageLink} {PreviousPageLink}   #{msj['btn.ir.a.pagina']}: {JumpToPageDropdown}  {NextPageLink} {LastPageLink}"
						currentPageReportTemplate="({currentPage} de {totalPages})" 
						paginatorPosition="bottom">

						<f:facet name="header">#{msj['label.titulo.historial.retroalimentacion']}</f:facet>

						<p:column headerText="Detalle" width="32">
							<p:rowToggler />
						</p:column>


						<p:column
							headerText="#{msj['column.propuestas.por.validar.prioridad']}"
							width="40">
							<div align="center">
								<h:outputText value="#{retroalimentacion.prioridadSugerida}" />
							</div>
						</p:column>

						<p:column headerText="#{msj['column.inicio']}" width="50">
							<div align="center">
								<h:outputText value="#{retroalimentacion.fechaInicioPeriodo}">
									<f:convertDateTime pattern="dd/MM/yyyy" />
								</h:outputText>
							</div>
						</p:column>

						<p:column headerText="#{msj['column.fin']}" width="50">
							<div align="center">
								<h:outputText value="#{retroalimentacion.fechaFinPeriodo}">
									<f:convertDateTime pattern="dd/MM/yyyy" />
								</h:outputText>
							</div>
						</p:column>

						<p:column headerText="#{msj['column.asignadas.tipo.propuesta']}"
							width="70">
							<div align="center">
								<h:outputText
									value="#{retroalimentacion.fececTipoPropuesta.descripcion}">
									<f:convertDateTime pattern="dd/MM/yyyy" />
								</h:outputText>
							</div>
						</p:column>

						<p:column
							headerText="#{msj['column.propuestas.por.validar.fecha.atencion']}"
							width="80">
							<div align="center">
								<h:outputText value="#{retroalimentacion.fechaCreacion}">
									<f:convertDateTime pattern="dd/MM/yyyy" />
								</h:outputText>
							</div>
						</p:column>

						<p:column
							headerText="#{msj['label.impuesto.historial.retroalimentacion']}"
							width="70">

							<div align="center">
								<p:commandLink value="#{msj['label.titulo.ver.impuesto']}"
									ajax="true"
									action="#{consultarPropuestasAsignadasManagedBean.obtenerImpuestosRetro}"
									immediate="true" update=":formPropuesta:pnlImpuestoHistorico"
									style="color:#0170b7;">
									<f:setPropertyActionListener value="#{retroalimentacion}"
										immediate="true"
										target="#{consultarPropuestasAsignadasManagedBean.consultarPropAsignadaHelper.solicitudRetroalimentacion}" />
								</p:commandLink>
							</div>

						</p:column>


						<p:column 
							width="55">
							
							  <f:facet name="header">
                                 <h:outputText id="idNotaInformativa" value="#{msj['label.historico.nota.informativa']}" title="#{msj['toltip.nota.informativa']}"/>
                              </f:facet>
							
							<div align="center">
								<p:commandLink
									value="#{retroalimentacion.numeroDocRetroalimentacion}"
									ajax="true"
									action="#{consultarPropuestasAsignadasManagedBean.obtenerDocumentosRetroalimentados}"
									immediate="true" update=":formPropuesta:pnlDocumentoHistorcio"
									style="color:#0170b7;">
									<f:setPropertyActionListener value="#{retroalimentacion}"
										immediate="true"
										target="#{consultarPropuestasAsignadasManagedBean.consultarPropAsignadaHelper.solicitudRetroalimentacion}" />
								</p:commandLink>
							</div>

						</p:column>
						
						<p:tooltip id="toolTipNotaInf" for="idNotaInformativa" value="#{msj['toltip.nota.informativa']}" />
			
						<p:column
							headerText="#{msj['label.prpuestas.validar.retro.solicitud']}"
							width="80">
							<div align="center">
								<p:commandLink
									action="#{consultarPropuestasAsignadasManagedBean.obtenerSolictudRetroalimentacion}"
									update=":formPropuesta:idHistoricoRetroalimentacion"
									inmediate="true" width="120">
									<h:graphicImage ajax="true"
										value="/resources/images/document.png" />
									<f:setPropertyActionListener value="#{retroalimentacion}"
										target="#{consultarPropuestasAsignadasManagedBean.consultarPropAsignadaHelper.solicitudRetroalimentacion}" />
								</p:commandLink>

							</div>
						</p:column>

						<p:rowExpansion>
							<h:panelGrid id="displayHistorico" columns="2" cellpadding="10"
								style="width:600px;" styleClass=" ui-widget-content grid">
								<h:outputText value="#{msj['label.metodo']}"/>
								<h:outputText value="#{retroalimentacion.feceaMetodo.abreviatura}" />
								<h:outputText value="#{msj['label.causa.programacion']}" />
								<h:outputText
									value="#{retroalimentacion.fececCausaProgramacion.descripcion}" />
								<h:outputText
									value="#{msj['column.origen.central.subprograma']}" />
								<h:outputText
									value="#{retroalimentacion.fececSubprograma.clave} - #{retroalimentacion.fececSubprograma.descripcion}" />
								<h:outputText
									value="#{msj['column.propuestas.por.validar.sector']}" />
								<h:outputText
									value="#{retroalimentacion.fececSector.descripcion}" />
								<h:outputText
									value="#{msj['label.seguimiento.motivo.retroalimentacion']}" />
								<h:outputText
									value="#{retroalimentacion.fececMotivo.descripcion}" />
							</h:panelGrid>
						</p:rowExpansion>

					</p:dataTable>
				</p:panel>

			</p:fieldset>
			<br />
			<br />
			<p:fieldset id="tolgateOrden" widgetVar="tolgateOrden" legend="Historial de documento orden" toggleable="true"
				toggleSpeed="500" collapsed="true">
				<p:ajax event="toggle"
					listener="#{consultarPropuestasAsignadasManagedBean.handleToggleExpedienteOrden}"
					update=":formPropuesta:tablaHistoricoOrden :formPropuesta:tablaHistoricoOrdenOficio " />
				<p:dataTable id="tablaHistoricoOrden" var="documento"
					value="#{consultarPropuestasAsignadasManagedBean.consultarPropAsignadaHelper.listaHistorialOrden}"
					editable="false" emptyMessage="#{msj['label.lista.vacia']}"
					paginator="true" rows="5"
					paginatorTemplate="#{msj['btn.pagina']}: {CurrentPageReport} {FirstPageLink} {PreviousPageLink}   #{msj['btn.ir.a.pagina']}: {JumpToPageDropdown}  {NextPageLink} {LastPageLink}"
					currentPageReportTemplate="({currentPage} de {totalPages})"
					paginatorPosition="bottom">

					<f:facet name="header">#{msj['label.propuestas.doctos.orden']}</f:facet>

					<p:column headerText="#{msj['column.fechaHora.orden']}"
						styleClass="someClass" style="width:80px;">
						<div align="center">
							<h:outputText value="#{documento.fechaHora}" style="width:80px;">
								<f:convertDateTime pattern="dd/MM/yyyy HH:mm:ss" locale="es_MX"
									timeZone="America/Mexico_City" />
							</h:outputText>
						</div>
					</p:column>
					<p:column headerText="#{msj['column.asignadas.expediente']}"
						style="width:130px;margin:auto;">
						<div align="center">
							<h:outputText value="#{documento.nombreArchivo}" />
						</div>
					</p:column>
					<p:column headerText="#{msj['label.encabezado.documento']}"
						style="width:50px;">
						<div align="center">
							<h:commandButton image="/resources/images/document.png"
								title="Documento" ajax="true" style="padding:0px 5px;">
								<f:setPropertyActionListener value="#{documento.rutaArchivo}"
									target="#{consultarPropuestasAsignadasManagedBean.consultarPropAsignadaHelper.rutaArchivo}" />
								<f:setPropertyActionListener value="#{documento.nombreArchivo}"
									target="#{consultarPropuestasAsignadasManagedBean.consultarPropAsignadaHelper.nombreArchivo}" />
								<p:fileDownload contentDisposition="attachment"
									value="#{consultarPropuestasAsignadasManagedBean.descargarArchivo}" />
							</h:commandButton>
						</div>
					</p:column>

					<p:column headerText="#{msj['label.usuario.accion.historial']}"
						style="width:130px;margin:auto;">
						<div align="center">
							<h:outputText value="#{documento.descripcionEmpleado}" />
						</div>
					</p:column>



				</p:dataTable>
				<br />
				<br />

				<p:dataTable id="tablaHistoricoOrdenOficio"
					value="#{consultarPropuestasAsignadasManagedBean.consultarPropAsignadaHelper.listaHistorialOficio}"
					var="oficio" editable="false"
					emptyMessage="#{msj['label.lista.vacia']}" rows="5"
					paginator="true"
					paginatorTemplate="#{msj['btn.pagina']}: {CurrentPageReport} {FirstPageLink} {PreviousPageLink}   #{msj['btn.ir.a.pagina']}: {JumpToPageDropdown}  {NextPageLink} {LastPageLink}"
					currentPageReportTemplate="({currentPage} de {totalPages})"
					paginatorPosition="bottom">
					<f:facet name="header">
						<h:outputLabel value="#{msj['consultar.tableoficio.titulo']}"
							style="font-weight:bold" />
					</f:facet>
					<p:column headerText="#{msj['column.fecha.hora.carga.documento']}"
						style="width:80px;">
						<div align="center">
							<h:outputText value="#{oficio.fechaHora}">
								<f:convertDateTime pattern="dd/MM/yyyy HH:mm:ss" locale="es_MX"
									timeZone="America/Mexico_City" />
							</h:outputText>
						</div>
					</p:column>
					<p:column headerText="#{msj['column.label.oficio']}"
						style="width:130px;margin:auto;">
						<div align="center">
							<h:outputText value="#{oficio.nombreArchivo}" />
						</div>
					</p:column>
					<p:column headerText="#{msj['label.encabezado.documento']}"
						style="width:50px;">
						<div align="center">
							<h:commandButton image="/resources/images/document.png"
								title="Documento" ajax="true" style="padding:0px 5px;">
								<f:setPropertyActionListener value="#{oficio.rutaArchivo}"
									target="#{consultarPropuestasAsignadasManagedBean.consultarPropAsignadaHelper.rutaArchivo}" />
								<f:setPropertyActionListener value="#{oficio.nombreArchivo}"
									target="#{consultarPropuestasAsignadasManagedBean.consultarPropAsignadaHelper.nombreArchivo}" />
								<p:fileDownload contentDisposition="attachment"
									value="#{consultarPropuestasAsignadasManagedBean.descargarArchivo}" />
							</h:commandButton>
						</div>
					</p:column>
					<p:column headerText="#{msj['label.usuario.accion.historial']}"
						style="width:130px;margin:auto;">
						<div align="center">
							<h:outputText value="#{oficio.descripcionEmpleado}" />
						</div>
					</p:column>
				</p:dataTable>

			</p:fieldset>

			<!-- Botones de accion -->

			<h:panelGroup
				rendered="#{!consultarPropuestasAsignadasManagedBean.cpaBooleanHelper.muestraBotonesAccion}">
				<p:commandButton value="#{msj['btn.cancelar.propuesta']}"
					update=":formPropuesta @form"
					oncomplete="ocultaMsgAgace('msgExitoGuardar');"
					action="#{consultarPropuestasAsignadasManagedBean.llamaAnalizarCancelacionPropuesta}" />

				<p:commandButton value="#{msj['btn.rechazar']}"
					update=":formPropuesta @form"
					oncomplete="ocultaMsgAgace('msgExitoGuardar');"
					action="#{consultarPropuestasAsignadasManagedBean.llamaAnalizarRechazoPropuesta}" />

				<p:commandButton value="#{msj['btn.transferir']}"
					update="@form :formPropuesta"
					oncomplete="ocultaMsgAgace('msgExitoGuardar');"
					action="#{consultarPropuestasAsignadasManagedBean.llamaAnalizarTransferirPropuesta}" />

				<p:commandButton value="#{msj['btn.retroalimentar']}"
					update="@form :formPropuesta"
					action="#{consultarPropuestasAsignadasManagedBean.llamaAnalizarRetroalimentarPropuesta}" />
				<p:spacer width="250px" height="30px" />
				<p:commandButton value="#{msj['btn.actualizar.orden']}"
					update="@form :formPropuesta"
					oncomplete="ocultaMsgAgace('msgExitoGuardar');"
					action="#{consultarPropuestasAsignadasManagedBean.llamaActualizarOrden}"
					disabled="#{!consultarPropuestasAsignadasManagedBean.cpaBooleanHelper.muestraActualizaOrden}" />
				<p:commandButton value="#{msj['btn.revision']}"
					update="@form :formPropuesta:msgAdicional"
					oncomplete="ocultaMsgAgace('msgAdicional');"
					action="#{consultarPropuestasAsignadasManagedBean.generarRevision}">
					<f:setPropertyActionListener
						target="#{cargaDocumentoElectronicoMB.propuestaSeleccionada}"
						value="#{consultarPropuestasAsignadasManagedBean.cpaDtoHelper.propuestaSelAnalizar}" />
					<f:setPropertyActionListener
						target="#{cargaDocumentoElectronicoMB.estatus1}"
						value="#{consultarPropuestasAsignadasManagedBean.cpaParamHelper.estatusPropuesta1}" />
					<f:setPropertyActionListener
						target="#{cargaDocumentoElectronicoMB.estatus2}"
						value="#{consultarPropuestasAsignadasManagedBean.cpaParamHelper.estatusPropuesta2}" />
					<f:setPropertyActionListener
						target="#{cargaDocumentoElectronicoMB.accion}"
						value="#{consultarPropuestasAsignadasManagedBean.cpaParamHelper.accionPropuesta}" />
				    <f:setPropertyActionListener
						target="#{cargaDocumentoElectronicoMB.empleado}"
						value="#{consultarPropuestasAsignadasManagedBean.empleadoLogueado}" />
				</p:commandButton>

			</h:panelGroup>
			<br />
			<br />
			<p:commandButton value="#{msj['btn.pantalla.anterior']}"
				style="width:120px;" 
				action="#{consultarPropuestasAsignadasManagedBean.listadoPropuestas}"
				rendered="#{!consultarPropuestasAsignadasManagedBean.cpaBooleanHelper.vieneConsultaPropOrd}"
				update=":formPropuestas :formPropuesta" />
			<p:commandButton value="#{msj['btn.pantalla.anterior']}"
				style="width:120px;"
				rendered="#{consultarPropuestasAsignadasManagedBean.cpaBooleanHelper.vieneConsultaPropOrd}"
				action="#{consultarPropuestasAsignadasManagedBean.redireccionaRegreso}"
			    ajax="true" proces="@this" update="@form"/>				    
			<p:spacer width="535px" height="5px" />
			<p:commandButton value="#{msj['btn.asignar.consulta']}"
				style="width:120px;" update=":formPropuesta @form"
				oncomplete="ocultaMsgAgace('msgAdicional');"
				actionListener="#{consultarPropuestasAsignadasManagedBean.cargaAsignarConsulta}"
				rendered="#{!consultarPropuestasAsignadasManagedBean.cpaBooleanHelper.muestraBotonesAccion}" />

		</p:panel>

		<!-- Panel de Comparacion de Datos del Contribuyente -->
		<p:panel id="panelDifDatos" styleClass="panel-interno" header="Información"
			rendered="#{consultarPropuestasAsignadasManagedBean.cpaBooleanHelper.muestraContribuyenteAnterior}">

			<ui:include src="/propuestas/auditor/datosContribuyente.jsf" />
		</p:panel>

		<!-- Formulario Cancelar Propuestas -->
		<p:panel id="panelCancelarPropuesta" header="Cancelar Propuesta"
			rendered="#{consultarPropuestasAsignadasManagedBean.cpaBooleanHelper.muestraFormularioCancelar}">

			<ui:include src="/propuestas/auditor/formularioCancelar.jsf" />
		</p:panel>

		<!-- Formulario Rechazo Propuestas-->
		<p:panel id="panelRechazoPropuesta"
			header="#{msj['header.panel.rechazo.propuesta']}"
