			<p:spacer width="1" height="20" />
		</p:dialog>

		<p:dialog header="#{msj['label.confirm.header']}"
			widgetVar="confirmarMediosContacto" resizable="false" modal="true"
			width="400" height="80">
			<h:panelGroup layout="block" style="padding:5px;"
				id="msjeConfirmarMediosContacto">
				<h:outputText
					value="No se encuentran Medios de Contacto. ¿Desea capturar el E-mail del Agente Aduanal?" />
			</h:panelGroup>
			<p:commandButton value="#{msj['btn.no']}"
				onclick="PF('confirmarMediosContacto').hide();"
				action="#{consultarPropuestasAsignadasManagedBean.deshabilitaEmailColaborador}"
				styleClass="ui-confirmdialog-no" icon="ui-icon-close"
				style="float:right;" update="@form" immediate="true" />
			<p:commandButton value="#{msj['btn.si']}"
				onclick="PF('confirmarMediosContacto').hide();"
				action="#{consultarPropuestasAsignadasManagedBean.habilitaEmailColaborador}"
				styleClass="ui-confirmdialog-yes" icon="ui-icon-check"
				style="float:right;" update="@form" immediate="true" />
		</p:dialog>

		<p:dialog widgetVar="noIdc" resizable="false" modal="true" width="400"
			height="80">
			<h:panelGroup layout="block" style="padding:5px;" id="msjeNoIdc">
				<h:outputText value="La información solicitada no existe en IDC." />
			</h:panelGroup>
			<p:commandButton value="Aceptar" onclick="PF('noIdc').hide();"
				styleClass="ui-confirmdialog-no" icon="ui-icon-close"
				style="float:right;" update="@form" />
		</p:dialog>

	</h:form>

	<h:outputScript library="js/" name="agace.js" />

	<h:outputStylesheet library="css/" name="agace.css" />
</h:body>
</html>