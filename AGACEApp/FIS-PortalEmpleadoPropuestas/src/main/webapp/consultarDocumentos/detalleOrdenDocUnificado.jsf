<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<f:view xmlns:f="http://java.sun.com/jsf/core"
        xmlns:h="http://java.sun.com/jsf/html">
    <html xmlns="http://www.w3.org/1999/xhtml"
          xmlns:p="http://primefaces.org/ui">

        <h:head></h:head>
        <h:body>
            <h:form id="detalleOrdenDocUnif" styleClass="panel-principal" >

                <p:ajaxStatus onstart="PF('statusDialog').show();"
                              onsuccess="PF('statusDialog').hide();" />
                <p:dialog modal="true" widgetVar="statusDialog"
                          header="#{msj['titulo.panel.espere']}" draggable="false"
                          closable="false" resizable="false">
                    <p:graphicImage value="/resources/images/ajaxloadingbar.gif" />
                </p:dialog>
                <p:messages id="msgExitoGuardar" showDetail="true" globalOnly="true"
                            closable="true" />
                <p:panel id="panelDocumentacionOrden" header="Documentación de la orden">

                    <div align="left"  >

                        <h:panelGrid rendered="true" >
                            <h:panelGrid columns="6" cellspacing="10">
                                <p:column>
                                    <div align="right" style="width:80px">					
                                        <h:outputText value="#{msj['consultar.label.numeroorden']}:" style="font-weight:bold;"/>
                                    </div>					
                                    <br/>
                                    <div align="right" style="width:80px">
                                        <h:outputText value="#{msj['consultar.label.idregistro']}:" style="font-weight:bold;"/>
                                    </div>							
                                </p:column>
                                <p:column>
                                    <div align="left" >
                                        <h:outputText value="#{detalleOrdenUnificadoMB.ordenConsultaSeleccionada.numeroOrden}" />
                                    </div>
                                    <br/>
                                    <br/>
                                    <div align="left">
                                        <h:outputText value="#{detalleOrdenUnificadoMB.ordenConsultaSeleccionada.idRegistro}" />
                                    </div>	
                                </p:column>
                                <p:column>
                                    <br/>
                                    <div align="right" style="width:110px">
                                        <h:outputText value="#{msj['consultar.label.metodo']}:" style="font-weight:bold;"/>
                                    </div>
                                    <br/>
                                    <br/>
                                    <div align="right" style="width:110px">
                                        <h:outputText value="#{msj['label.plazo.concluir.orden']}:" style="font-weight:bold;"/>
                                    </div>
                                </p:column>
                                <p:column>
                                    <div align="left">
                                        <h:outputText value="#{detalleOrdenUnificadoMB.ordenConsultaSeleccionada.metodo}" />
                                    </div>
                                    <br/>
                                    <br/>
                                    <div align="left">
                                        <h:outputText value="#{detalleOrdenUnificadoMB.ordenConsultaSeleccionada.descripcionPlazoRestante}" />
                                    </div>
                                </p:column>
                                <p:column>
                                    <div align="right" style="width:100px">
                                        <h:outputText value="#{msj['consultar.label.rfc.contribuyente']}:" style="font-weight:bold;"/>
                                    </div>
                                    <br/>
                                    <br/>
                                    <div align="right" style="width:100px">
                                        <h:outputText value="#{msj['consultar.label.razon']}:" style="font-weight:bold;"/>
                                    </div>										
                                </p:column>
                                <p:column>
                                    <div align="right">
                                        <h:outputText value="#{detalleOrdenUnificadoMB.helper.contribuyente.rfc}" />
                                    </div>
                                    <br/>
                                    <br/>
                                    <div align="right">
                                        <h:outputText value="#{detalleOrdenUnificadoMB.helper.contribuyente.nombre}" />
                                    </div>	
                                </p:column>
                            </h:panelGrid>
                        </h:panelGrid>		
                    </div>




                    <f:facet name="header">
                        <h:outputText value="#{msj['consultar.tabledocrelacionada.titulo']}" />
                    </f:facet>

                    <h:panelGrid>
                        <p:accordionPanel id="pnlConsultarDocRelacionada" rendered="#{detalleOrdenUnificadoMB.vistaHelper.pnlDocRequerida}" styleClass="panel-interno" style="width:99% !important; height:99% !important;">
                            <p:tab title="#{msj['consultar.tablepromocion.titulo']}">

                                <p:dataTable var="promocionConsulta" style="display:block; text-align:center" 
                                             value="#{detalleOrdenUnificadoMB.ordenConsultaSeleccionada.listPromocionConsultaDTO}"
                                             editable="false" emptyMessage="#{msj['label.lista.vacia']}" paginator="true" rows="5" paginatorTemplate="Página: {CurrentPageReport} {FirstPageLink}                                     
                                             {PreviousPageLink} &amp;nbsp; Ir a página: {JumpToPageDropdown} &amp;nbsp; {NextPageLink} {LastPageLink}" currentPageReportTemplate="({currentPage} de {totalPages})" paginatorPosition="bottom">
                                    <f:facet name="header">
                                        <div align="left">
                                            <h:outputText value="#{msj['consultar.tablepromocion.titulo']}" />
                                        </div>
                                    </f:facet>

                                    <p:column headerText="#{msj['consultar.tablepromocion.id']}">
                                        <h:outputText value="#{promocionConsulta.idPromocion}" />
                                    </p:column>

                                    <p:column headerText="#{msj['consultar.tablepromocion.tipo']}">
                                        <h:outputText value="#{promocionConsulta.tipoPromocionStr}" />
                                    </p:column>

                                    <p:column headerText="#{msj['consultar.tablepromocion.fechaenvio']}">
                                        <h:outputText value="#{promocionConsulta.fechaEnvio}">
                                            <f:convertDateTime pattern="dd/MM/yyyy" />
                                        </h:outputText>
                                    </p:column>

                                    <p:column headerText="#{msj['consultar.tablepromocion.promocion']}">
                                        <h:commandButton ajax="true"
                                                         image="/resources/images/document.png" process="@this"
                                                         title="#{promocionConsulta.nombreArchivo}">
                                            <f:setPropertyActionListener
                                                target="#{detalleOrdenUnificadoMB.rutaArchivo}"
                                                value="#{promocionConsulta.rutaArchivo}" />
                                            <f:setPropertyActionListener
                                                target="#{detalleOrdenUnificadoMB.nombreArchivo}"
                                                value="#{promocionConsulta.nombreArchivo}" />
                                            <p:fileDownload contentDisposition="attachment"
                                                            value="#{detalleOrdenUnificadoMB.archivoPromocion}" />
                                        </h:commandButton>
                                    </p:column>
                                    <p:column headerText="#{msj['consultar.tablepromocion.doccontri']}">
                                        <p:commandLink id="linkDocPruebasAlegatos"
                                                       action="#{detalleOrdenUnificadoMB.buscarDocPruebasAlegatos}"
                                                       ajax="true" update=":detalleOrdenDocUnif:pnlDocPruebaAlegatos">
                                            <f:setPropertyActionListener
                                                target="#{detalleOrdenUnificadoMB.helper.promocionConsultaSeleccionada}"
                                                value="#{promocionConsulta}" />
                                            <h:outputText
                                                value="#{promocionConsulta.documentosContribuyente}" />
                                        </p:commandLink>

                                    </p:column>

                                    <p:column headerText="#{msj['consultar.tablepromocion.extemporaneo']}">
                                        <p:outputPanel rendered="#{promocionConsulta.extemporaneo}">
                                            <h:commandButton image="/resources/images/ok.png" />
                                        </p:outputPanel>
                                        <p:outputPanel rendered="#{!promocionConsulta.extemporaneo}">
                                            <h:commandButton image="/resources/images/cancel.png" />
                                        </p:outputPanel>
                                    </p:column>
                                    <p:column headerText="#{msj['consultar.tablepromocion.presentadopor']}">
                                        <h:outputText value="#{promocionConsulta.presentadoPor}" />
                                    </p:column>
                                </p:dataTable>
                            </p:tab>
                        </p:accordionPanel>
                        <!--************************* TABLA DE OFICIO **********************************-->
                        <p:accordionPanel id="pnlOficios" rendered="#{detalleOrdenUnificadoMB.vistaHelper.pnlOficios}" styleClass="panel-interno" style="width:99% !important; height:99% !important;">
                            <p:tab title="#{msj['consultar.tableoficio.titulo']}">

                                <p:spacer height="30px" />					
                                <p:dataTable var="oficioConsulta"
                                             value="#{detalleOrdenUnificadoMB.ordenConsultaSeleccionada.listOficioConsultaDTO}" style="display:block; text-align:center" editable="false" 
                                             emptyMessage="#{msj['label.lista.vacia']}" paginator="true" rows="5" paginatorTemplate="Página: {CurrentPageReport} {FirstPageLink}                                     
                                             {PreviousPageLink} &amp;nbsp; Ir a página: {JumpToPageDropdown} &amp;nbsp; {NextPageLink} {LastPageLink}" currentPageReportTemplate="({currentPage} de {totalPages})" paginatorPosition="bottom">
                                    <f:facet name="header">
                                        <div align="left">
                                            <h:outputText value="#{msj['consultar.tableoficio.titulo']}" />
                                        </div>
                                    </f:facet>
                                    <p:column headerText="#{msj['consultar.tableoficio.idoficio']}">
                                        <p:outputPanel >
                                            <p:commandLink id="linkOficiosDeRequerimientos"
                                                           action="#{detalleOrdenUnificadoMB.redireccionaOficio}"
                                                           ajax="true" proces="@this" update="@form">
                                                <f:setPropertyActionListener
                                                    target="#{detalleOrdenUnificadoMB.helper.oficioConsultaSeleccionado}"
                                                    value="#{oficioConsulta}" />
                                                <h:outputText value="#{oficioConsulta.idOficio}" />
                                            </p:commandLink>
                                        </p:outputPanel>
                                    </p:column>
                                    <p:column headerText="#{msj['consultar.tableoficio.tipo']}">
                                        <h:outputText value="#{oficioConsulta.tipoOficio}" />
                                    </p:column>
                                    <p:column headerText="#{msj['consultar.tableoficio.oficio']}">
                                        <h:commandButton ajax="true"
                                                         image="/resources/images/document.png" process="@this"
                                                         title="#{oficioConsulta.nombreArchivo}">
                                            <f:setPropertyActionListener
                                                target="#{detalleOrdenUnificadoMB.rutaArchivo}"
                                                value="#{oficioConsulta.rutaArchivo}" />
                                            <f:setPropertyActionListener
                                                target="#{detalleOrdenUnificadoMB.nombreArchivo}"
                                                value="#{oficioConsulta.nombreArchivo}" />
                                            <p:fileDownload contentDisposition="attachment"
                                                            value="#{detalleOrdenUnificadoMB.archivoOficio}" />
                                        </h:commandButton>
                                    </p:column>
                                    <p:column headerText="#{msj['consultar.tableoficio.fecnoti']}">
                                        <h:outputText value="#{oficioConsulta.fechaNotificacion}" >
                                            <f:convertDateTime pattern="dd/MM/yyyy" />
                                        </h:outputText>
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.anexos.oficio']}">
                                        <p:commandLink id="linkAnexosOficios"
                                                       action="#{detalleOrdenUnificadoMB.buscarAnexosOficios}"
                                                       ajax="true" proces="@this" update="@form">
                                            <f:setPropertyActionListener
                                                target="#{detalleOrdenUnificadoMB.helper.oficioConsultaSeleccionado}"
                                                value="#{oficioConsulta}" />
                                            <h:outputText value="#{oficioConsulta.anexos}" />
                                        </p:commandLink>

                                    </p:column>
                                    <p:column headerText="#{msj['consultar.tableoficio.feccarga']}">
                                        <h:outputText value="#{oficioConsulta.fechaCarga}" >
                                            <f:convertDateTime pattern="dd/MM/yyyy" />
                                        </h:outputText>
                                    </p:column>
                                    <p:column headerText="#{msj['consultar.tableoficio.fecfirma']}">
                                        <h:outputText value="#{oficioConsulta.fechaFirma}" >
                                            <f:convertDateTime pattern="dd/MM/yyyy" />
                                        </h:outputText>
                                    </p:column>
                                    <p:column headerText="#{msj['consultar.tableoficio.oficdepen']}">
                                        <p:commandLink id="linkOficiosDependientes"
                                                       action="#{detalleOrdenUnificadoMB.buscarOficiosDependientes}"
                                                       style="color:#0170b7;" ajax="true" onclick="PF('dlgOficiosDependientes').show();" immediate="true" 
                                                       update=":detalleOrdenDocUnif:pnlOficiosDependientes">
                                            <f:setPropertyActionListener
                                                target="#{detalleOrdenUnificadoMB.helper.oficioConsultaSeleccionado}"
                                                value="#{oficioConsulta}" />
                                            <f:setPropertyActionListener
                                                target="#{detalleOrdenUnificadoMB.tipoPanel}"
                                                value="1" />
                                            <h:outputText value="#{oficioConsulta.oficiosDependientes}" />
                                        </p:commandLink>

                                    </p:column>
                                </p:dataTable>
                            </p:tab>
                        </p:accordionPanel>

                        <!-- Inicio de bloque de Historicos de Oficios Rechazados -->
                        <p:accordionPanel id="panelDatosHistorico" rendered="#{detalleOrdenUnificadoMB.vistaHelper.pnlOficiosHist}" styleClass="panel-interno" style="width:99% !important; height:99% !important;">
                            <p:tab title="#{msj['titulo.tabla.historico.oficio.rechazo']}">

                                <p:dataTable id="tablaHistoricoOficiosRechazados" var="documentacion" value="#{detalleOrdenUnificadoMB.helper.listaOficiosRechazados}" 
                                             editable="false" emptyMessage="#{msj['label.lista.vacia']}" paginator="true" rows="5" 
                                             paginatorTemplate="Página: {CurrentPageReport} {FirstPageLink} {PreviousPageLink} &amp;nbsp; Ir a página: {JumpToPageDropdown} &amp;nbsp; {NextPageLink} {LastPageLink}" 
                                             currentPageReportTemplate="({currentPage} de {totalPages})" paginatorPosition="bottom">
                                    <f:facet name="header">
                                        #{msj[&apos;titulo.tabla.historico.oficio.rechazo&apos;]}
                                    </f:facet>
                                    <p:column headerText="#{msj['column.label.id.oficio']}">
                                        #{documentacion.idOficio}
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.tipo.oficio']}" >
                                        #{documentacion.fecetTipoOficio.nombre}
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.oficio']}" >
                                        <div align="center">
                                            <h:commandButton image="/resources/images/document.png" title="#{documentacion.nombreArchivo}" ajax="true" immediate="true" style="padding:0px 5px;">
                                                <f:setPropertyActionListener value="#{documentacion}" target="#{detalleOrdenUnificadoMB.helper.ofRechazadoSeleccionado}"/>
                                                <p:fileDownload contentDisposition="attachment" value="#{detalleOrdenUnificadoMB.rechazoOficio}"/>
                                            </h:commandButton>
                                        </div>
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.fecha.rechazo']}" >
                                        <h:outputLabel value="#{documentacion.fecetRechazoOficio.fechaRechazo}">
                                            <f:convertDateTime pattern="dd/MM/yyyy"/>
                                        </h:outputLabel>
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.fecha.carga.oficio']}" >
                                        <h:outputLabel value="#{documentacion.fechaCreacion}">
                                            <f:convertDateTime pattern="dd/MM/yyyy"/>
                                        </h:outputLabel>
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.anexos.oficio']}" >
                                        <div align="center">
                                            <p:commandLink action="#{detalleOrdenUnificadoMB.getAnexosOficioRechazo}" value="#{documentacion.totalAnexosOficio}" 
                                                           style="color:#0170b7;" ajax="true" onclick="PF('dlgAnexosOficioRechazo').show();" immediate="true" 
                                                           update=":detalleOrdenDocUnif:panelAnexosOficioRechazo">
                                                <f:setPropertyActionListener
                                                    target="#{detalleOrdenUnificadoMB.tipoPanel}"
                                                    value="2" />
                                                <f:setPropertyActionListener value="#{documentacion}" target="#{detalleOrdenUnificadoMB.helper.oficioSeleccionado}"/>
                                            </p:commandLink>
                                        </div>
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.oficios.dependientes']}" >
                                        <div align="center">
                                            <p:commandLink action="#{detalleOrdenUnificadoMB.getOficiosDependientesEstatusOficioRechazado}" value="#{documentacion.totalOficiosDependientes}" style="color:#0170b7;" ajax="true" onclick="PF('dlgHistoricoOficiosDependientes').show();" immediate="true" update=":detalleOrdenDocUnif:tablaHistoricoDlgOficiosDependientes">
                                                <f:setPropertyActionListener
                                                    target="#{detalleOrdenUnificadoMB.tipoPanel}"
                                                    value="2" />
                                                <f:setPropertyActionListener value="#{documentacion}" target="#{detalleOrdenUnificadoMB.helper.oficioSeleccionado}"/>
                                            </p:commandLink>
                                        </div>
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.motivo.rechazo']}" >
                                        <h:outputText value="#{documentacion.fecetRechazoOficio.descripcion}"/>
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.documentos.rechazo.archivo']}">
                                        <p:commandLink action="#{detalleOrdenUnificadoMB.getAnexosRechazoOficio}" value="#{documentacion.totalAnexosRechazo}" style="color:#0170b7;" ajax="true" onclick="PF('dlgAnexosRechazoOficio').show();" immediate="true" update=":detalleOrdenDocUnif:tablaDlgAnexosRechazoOficio">
                                            <f:setPropertyActionListener
                                                target="#{detalleOrdenUnificadoMB.tipoPanel}"
                                                value="2" />
                                            <f:setPropertyActionListener value="#{documentacion}" target="#{detalleOrdenUnificadoMB.helper.oficioSeleccionado}"/>
                                        </p:commandLink>                            
                                    </p:column>
                                </p:dataTable>
                            </p:tab>
                        </p:accordionPanel>					

                        <!--************************* TABLA DE SOLICITUDES DE COTRIBUYENTE *******************************************-->
                        <p:accordionPanel id="panelDatosSolicitudContr" rendered="#{detalleOrdenUnificadoMB.vistaHelper.pnlSolContrib}" styleClass="panel-interno" style="width:99% !important; height:99% !important;">                    
                            <p:tab title="#{msj[&apos;consultar.pnl.documentacion.solicitud.contrib.titulo&apos;]}">					
                                <p:dataTable id="tablaSolicitudContr" var="documentacion" value="#{detalleOrdenUnificadoMB.solHelper.listaSolicitudContribuyenteConcentrado}" 
                                             editable="false" emptyMessage="#{msj['label.lista.vacia']}" paginator="true" rows="5" paginatorTemplate="Página: {CurrentPageReport} {FirstPageLink}                                     
                                             {PreviousPageLink} &amp;nbsp; Ir a página: {JumpToPageDropdown} &amp;nbsp; {NextPageLink} {LastPageLink}" currentPageReportTemplate="({currentPage} de {totalPages})" paginatorPosition="bottom">
                                    <f:facet name="header">
                                        #{msj[&apos;consultar.pnl.documentacion.solicitud.contrib.titulo&apos;]}
                                    </f:facet>
                                    <p:column headerText="#{msj['label.id.solicitud.contribuyente']}" style="width:10%">
                                        #{documentacion.id}
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.fecha.solicitud']}" style="width:10%">
                                        <h:outputLabel value="#{documentacion.fechaCarga}">
                                            <f:convertDateTime pattern="dd/MM/yyyy"/>
                                        </h:outputLabel>
                                    </p:column>
                                    <p:column headerText="#{msj['label.descripcion.solicitud.contribuyente']}" style="width:15%">
                                        <h:outputLabel value="#{documentacion.descripcion}"/>                                   
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.documentacion.contribuyente']}" style="width:15%">
                                        <div align="center">
                                            <p:commandLink action="#{detalleOrdenUnificadoMB.getDocumentacionSolicitudContribuyente}" value="#{documentacion.totalDocumentosContribuyente}" style="color:#0170b7;" ajax="true" 
                                                           onclick="PF('dlgDocSolicitudContr').show();" immediate="true" update=":detalleOrdenDocUnif:panelDocSolicitudCont">
                                                <f:setPropertyActionListener value="#{documentacion}" target="#{detalleOrdenUnificadoMB.solHelper.solicitudContribuyenteVOSeleccionada}"/>
                                            </p:commandLink>
                                        </div>
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.estatus']}" style="width:10%">
                                        <p:outputLabel value="#{documentacion.estatus}"/>
                                    </p:column>

                                    <p:column headerText="#{msj['column.label.justificacion']}" style="width:15%">
                                        <h:outputText value="#{documentacion.justificacion}"/>
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.anexos.resolucion.solicitudes']}" style="width:15%">
                                        <div align="center">																																								
                                            <p:commandLink action="#{detalleOrdenUnificadoMB.getAnexosSolicitudContribuyente}" value="#{documentacion.totalDocumentosRechazo}" style="color:#0170b7;" ajax="true" onclick="PF('dlgAnexosSolicitudContr').show();" immediate="true" update=":detalleOrdenDocUnif:panelGrpAnexosSolicitudContr">
                                                <f:setPropertyActionListener value="#{documentacion}" target="#{detalleOrdenUnificadoMB.solHelper.solicitudContribuyenteVOSeleccionada}"/>
                                            </p:commandLink>
                                        </div>
                                    </p:column>
                                </p:dataTable>
                            </p:tab>
                        </p:accordionPanel>                    
                        <!--************************* TABLA DE SOLICITUDES DE COTRIBUYENTE HISTORCICO**********************************--> 
                        <p:accordionPanel id="panelDatosHistoricoSolicitudContr" rendered="#{detalleOrdenUnificadoMB.vistaHelper.pnlSolContribHist}" styleClass="panel-interno" style="width:99% !important; height:99% !important;">                    
                            <p:tab title="#{msj[&apos;titulo.tabla.historico.solicitudes.contribuyente&apos;]}">					
                                <p:dataTable id="tablaHistoricoSolicitudContr" var="documentacion" value="#{detalleOrdenUnificadoMB.solHelper.listaSolicitudContribuyenteConcentradoHistorico}" 
                                             editable="false" emptyMessage="#{msj['label.lista.vacia']}" paginator="true" rows="5" paginatorTemplate="Página: {CurrentPageReport} {FirstPageLink}                                     
                                             {PreviousPageLink} &amp;nbsp; Ir a página: {JumpToPageDropdown} &amp;nbsp; {NextPageLink} {LastPageLink}" currentPageReportTemplate="({currentPage} de {totalPages})" paginatorPosition="bottom">
                                    <f:facet name="header">
                                        #{msj[&apos;titulo.tabla.historico.solicitudes.contribuyente&apos;]}
                                    </f:facet>
                                    <p:column headerText="#{msj['label.id.solicitud.contribuyente']}" style="width:10%">
                                        <h:outputLabel value="#{documentacion.id}"/>
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.fecha.solicitud']}" style="width:10%">
                                        <h:outputLabel value="#{documentacion.fechaCarga}">
                                            <f:convertDateTime pattern="dd/MM/yyyy"/>
                                        </h:outputLabel>
                                    </p:column>
                                    <p:column headerText="#{msj['label.descripcion.solicitud.contribuyente']}" style="width:15%">
                                        <h:outputLabel value="#{documentacion.descripcion}"/>                                   
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.documentacion.contribuyente']}" style="width:15%">
                                        <div align="center">
                                            <p:commandLink action="#{detalleOrdenUnificadoMB.getDocumentacionSolicitudContribuyente}" value="#{documentacion.totalDocumentosContribuyente}" style="color:#0170b7;" ajax="true" 
                                                           onclick="PF('dlgDocSolicitudContr').show();" immediate="true" update=":detalleOrdenDocUnif:panelDocSolicitudCont">
                                                <f:setPropertyActionListener value="#{documentacion}" target="#{detalleOrdenUnificadoMB.solHelper.solicitudContribuyenteVOSeleccionada}"/>
                                            </p:commandLink>
                                        </div>
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.estatus']}" style="width:10%">
                                        <p:outputLabel value="#{documentacion.estatus}"/>
                                    </p:column>
                                    <p:column headerText="#{msj['column.label.documentos.resolucion.solicitudes']}" style="width:10%">
                                        <div align="center">
                                            <h:commandButton image="/resources/images/document.png"  ajax="true" immediate="true" style="padding:0px 5px;">
