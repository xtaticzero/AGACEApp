                                    <h:outputText value="#{documentoRechazo.nombre}" />
                                </p:column>
                                <p:column headerText="Descargar" style="25%;">                                 
                                    <div align="center">
                                        <h:commandButton ajax="true" image="/resources/images/document.png" style="padding:0px 40px;">
                                                <f:setPropertyActionListener value="#{documentoRechazo}" target="#{propuestasRechazadasVerifDoctoMB.propRechazadasHelper.documentoSeleccionadoRechazo}"/>
                                                <p:fileDownload contentDisposition="attachment" value="#{propuestasRechazadasVerifDoctoMB.documentoSeleccionDescargaRechazo}"/>
                                        </h:commandButton>
                                    </div>    
                                </p:column>
                            </p:dataTable>
                        </p:dialog>
                    
                    <!-- Documentos de la orden -->
                        <p:column rendered="true">
				<br/>
				<div align="center">
					<p:dataTable id="tablaDocumentacionOrden"
						value="#{propuestasRechazadasVerifDoctoMB.propRechazadasHelper.listaDocumentosOrden}"
						var="documentoOrden" editable="false"
						emptyMessage="#{msj['label.lista.vacia']}" paginator="true"
						rows="10"
						paginatorTemplate="Página: {CurrentPageReport} {FirstPageLink} {PreviousPageLink} &amp;nbsp; Ir a página: {JumpToPageDropdown} &amp;nbsp; {NextPageLink} {LastPageLink}"
