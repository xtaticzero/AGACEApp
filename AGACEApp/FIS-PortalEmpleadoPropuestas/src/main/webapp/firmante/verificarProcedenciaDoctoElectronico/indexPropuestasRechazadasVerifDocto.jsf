								<p:fileDownload contentDisposition="attachment"
									value="#{propuestasRechazadasVerifDoctoMB.documentoSeleccionDescargaOrden}" />
							</h:commandButton>
						</p:column>
					</p:dataTable>
				</div>
			</p:column>
                        
