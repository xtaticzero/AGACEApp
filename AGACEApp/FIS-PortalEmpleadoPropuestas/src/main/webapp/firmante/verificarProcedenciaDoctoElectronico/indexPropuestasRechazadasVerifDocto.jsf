                                    </table>
                                </td>
                              </tr>
                              <tr>
                                <td width="100%">
                                    <table border="0" width="100%" cellpadding="0" cellspacing="0">
                                          <tr><td><h:outputLabel value="#{msj['label.docElect.emailRepresentante']}" style="font-weight:bold"/></td></tr>
                                          <tr><td><p:inputText id="txtEmailRepresentante" style="width:325px;height:auto;" disabled="true" value="#{propuestasRechazadasVerifDoctoMB.propRechazadasHelper.representanteLegal.correo}"/></td></tr>
                                    </table>
                                </td>
                              </tr>
