                                          <tr><td><p:inputText id="txtRfcAgente" style="width:325px;height:auto;" disabled="true" value="#{propuestasRechazadasVerifDoctoMB.propRechazadasHelper.agenteAduanal.rfc}"/></td></tr>
                                    </table>
                                </td>
                              </tr>
                              <tr>
                                <td width="100%">
                                    <table border="0" width="100%" cellpadding="0" cellspacing="0">
                                          <tr><td><h:outputLabel value="#{msj['label.docElect.nombreRepresentante']}" style="font-weight:bold"/></td></tr>
                                          <tr><td><p:inputText id="txtNombreAgente" style="width:325px;height:auto;" disabled="true" value="#{propuestasRechazadasVerifDoctoMB.propRechazadasHelper.agenteAduanal.nombre}"/></td></tr>
                                    </table>
                                </td>
                              </tr>
                              <tr>
                                <td width="100%">
                                    <table border="0" width="100%" cellpadding="0" cellspacing="0">
                                          <tr><td><h:outputLabel value="#{msj['label.docElect.emailRepresentante']}" style="font-weight:bold"/></td></tr>
                                          <tr><td><p:inputText id="txtEmailAgente" style="width:325px;height:auto;" disabled="true" value="#{propuestasRechazadasVerifDoctoMB.propRechazadasHelper.agenteAduanal.correo}"/></td></tr>
