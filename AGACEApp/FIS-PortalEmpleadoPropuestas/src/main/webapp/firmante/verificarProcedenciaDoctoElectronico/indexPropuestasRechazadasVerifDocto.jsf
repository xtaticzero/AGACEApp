                                                                action="#{propuestasRechazadasVerifDoctoMB.visualizarDocumentos}" >                                    
                                        <f:setPropertyActionListener value="${listaRechazo.idRechazoPropuesta}" target="#{propuestasRechazadasVerifDoctoMB.propRechazadasHelper.idRechazoPropuesta}"/>
                                    </h:commandButton>
                                </p:column> 
                            </p:dataTable>
                        </p:column> 
