<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<f:view xmlns:f="http://java.sun.com/jsf/core" xmlns:h="http://java.sun.com/jsf/html">
    <html xmlns="http://www.w3.org/1999/xhtml" xmlns:p="http://primefaces.org/ui">
        <f:metadata>
            <f:event type="preRenderView" listener="#{asociarColaboradoresManagedBean.init}"/>
        </f:metadata>
        <h:head>
            <script>

            </script>
        </h:head>
        <h:body>
            <h:form id="formAsociar" styleClass="panel-principal">
                <p:ajaxStatus onstart="PF('statusDialogSeg').show();" onsuccess="PF('statusDialogSeg').hide();"/>
                <p:dialog modal="true" widgetVar="statusDialogSeg" header="#{msj['titulo.panel.espere']}"
                          draggable="false" closable="false" resizable="false">
                    <p:graphicImage value="/resources/images/ajaxloadingbar.gif"/>
                </p:dialog>
                <p:messages id="msgGeneral" showDetail="false" globalOnly="true" closable="true"/>
                <p:panel id="panelContribuyenteOrdenes"
                         style="border-bottom-right-radius:0;border-bottom-left-radius:0;" styleClass="panel-principal"
                         header="Órdenes" rendered="#{asociarColaboradoresManagedBean.ascColAttribHelper.monstrarPanelOrdenes}">
                    <p:panel styleClass="panel-secundario"
                             rendered="#{asociarColaboradoresManagedBean.ascColAttribHelper.mostrarDatosApoderadoLegal}">
                        <h:panelGrid border="0" columns="4">
                            <h:outputLabel value="Apoderado Legal." style="float:right;font-weight:bold"/>
                            <p:spacer width="500" height="1"/>
                        </h:panelGrid>
                        <p:separator/>
                        <h:panelGrid columns="2">    
                        <h:panelGrid columns="2" id="panelDatosApoderadoLegal" border="0" styleClass="panel-secundario">
                            <h:outputLabel value="#{msj['label.rfc']}" style="font-weight:bold"/>
                            <p:inputText id="txtApoderadoRFC" style="width:300px;"
                                         value="#{asociarColaboradoresManagedBean.ascColDTOHelper.apoderadoLegalVO.rfc}"
                                         disabled="#{asociarColaboradoresManagedBean.ascColDTOHelper.apoderadoLegalVO.deshabilitarCampos}">
                            </p:inputText>
                            <h:outputLabel value="#{msj['label.nombre.razon']}" style="font-weight:bold"/>
                            <p:inputText style="width:300px;"
                                         value="#{asociarColaboradoresManagedBean.ascColDTOHelper.apoderadoLegalVO.nombre}"
                                         disabled="#{asociarColaboradoresManagedBean.ascColDTOHelper.apoderadoLegalVO.deshabilitarCampos}"/>
                            <h:outputLabel value="#{msj['label.email']}" style="font-weight:bold"/>
                            <p:inputText style="width:300px;"
                                         value="#{asociarColaboradoresManagedBean.ascColDTOHelper.apoderadoLegalVO.correo}"
                                         disabled="#{asociarColaboradoresManagedBean.ascColDTOHelper.apoderadoLegalVO.deshabilitarCorreo}"/>
                            <h:outputLabel value="#{msj['label.medios.contacto']}" style="font-weight:bold"/>
                            <p:selectBooleanCheckbox value="#{asociarColaboradoresManagedBean.ascColDTOHelper.apoderadoLegalVO.medioContactoBoolean}"
                                                     disabled="true"/>
                            <p:spacer width="50" height="1"/>
	                        <p:spacer width="50" height="1"/>
	                        <h:outputLabel value="Órdenes sin Apoderado Legal:" style="font-weight:bold"/>
                            <p:selectBooleanCheckbox value="#{asociarColaboradoresManagedBean.ascColDTOHelper.apoderadoLegalVO.sinColaborador}"
                             disabled="#{asociarColaboradoresManagedBean.ascColAttribHelper.mostrarOrdenSinAL}">
                                <p:ajax update="@form" 
                                        listener="#{asociarColaboradoresManagedBean.validaSinApoderadoLegal}"/>
                                        
                            </p:selectBooleanCheckbox>
                             <p:spacer width="50" height="1"/>
                            </h:panelGrid>
                            	
                            	
                            <h:panelGrid columns="1" id="panelDatosAgenteLegalDocumentos" border="0" style="align:right">      
                            <p:commandButton value="#{msj['consultar.btn.buscar']} " styleClass="ui-confirmdialog-no"
                                             disabled="#{asociarColaboradoresManagedBean.ascColDTOHelper.apoderadoLegalVO.deshabilitarBotonBusquedaColaborador}"
                                             action="#{asociarColaboradoresManagedBean.buscarColaborador}"
                                             update="@form" 
                                             rendered="#{asociarColaboradoresManagedBean.ascColDTOHelper.apoderadoLegalVO.visibleBusquedaColaborador}"
                                             style="float:left;">
                                <f:setPropertyActionListener target="#{asociarColaboradoresManagedBean.ascColAttribHelper.tipoColaborador}"
                                                             value="AL"/>
                            </p:commandButton>
                            <p:commandButton value="#{msj['btn.nuevo.busqueda']} " styleClass="ui-confirmdialog-no"
                                             action="#{asociarColaboradoresManagedBean.nuevaBusquedaColaborador}"
                                             update="@form" 
                                             disabled="#{asociarColaboradoresManagedBean.ascColDTOHelper.apoderadoLegalVO.deshabilitarBotonBusquedaColaborador}"
                                             rendered="#{!asociarColaboradoresManagedBean.ascColDTOHelper.apoderadoLegalVO.visibleBusquedaColaborador}"
                                             style="float:left;">
                                <f:setPropertyActionListener target="#{asociarColaboradoresManagedBean.ascColAttribHelper.tipoColaborador}"
                                                             value="AL"/>
                            </p:commandButton>
                            <p:spacer width="100" height="1"/>
                            <p:commandButton value="Cancelar Busqueda" action="#{asociarColaboradoresManagedBean.cancelarNuevoColaborador}"
                                             update="@form" 
                                       style="float:left;" rendered="#{asociarColaboradoresManagedBean.ascColAttribHelper.mostrarCancelarBusquedaAL}">
                                     <f:setPropertyActionListener target="#{asociarColaboradoresManagedBean.ascColAttribHelper.tipoColaborador}"
                                                             value="AL"/>
                            </p:commandButton>
                            
                            <p:spacer width="100" height="1"/>
                            <p:commandButton value="Adjuntar documentos" onclick="PF('dlgDocApoderadoLegal').show();"
                                     type="button"  style="float:left;" update=":formAsociar:dlgDocApoderadoLegal" rendered="#{asociarColaboradoresManagedBean.ascColAttribHelper.mostrarAdjuntarDocumentoAL}" />
                                     
                            		<p:commandButton value="Ver documentos" onclick="PF('dlgDocApoderadoLegal').show();"
                                     type="button"  style="float:left;" update=":formAsociar:dlgDocApoderadoLegal" rendered="#{!asociarColaboradoresManagedBean.ascColAttribHelper.mostrarAdjuntarDocumentoAL}" />
                             <p:spacer width="100" height="1"/>         
                                     <p:commandButton value="#{msj['btn.guardar']}" styleClass="ui-confirmdialog-no" update="@form"
                                             action="#{asociarColaboradoresManagedBean.preGuardarColaborador}"
                                             rendered="#{asociarColaboradoresManagedBean.ascColAttribHelper.mostrarGuardarApoderadoLegal}"
                                             style="float:left;">
                                <f:setPropertyActionListener target="#{asociarColaboradoresManagedBean.ascColAttribHelper.tipoColaborador}"
                                                             value="AL"/>
                            </p:commandButton>
                                     
                        </h:panelGrid>
                        </h:panelGrid>
                    </p:panel>
                    <br/>
                    <br/>
                    <h:panelGrid border="0" columns="4">
                        <h:outputLabel value="Órdenes" style="float:right;font-weight:bold"/>
                        <p:spacer width="500" height="1"/>
                    </h:panelGrid>
                    <p:separator/>
                    <p:dataTable id="tablaOrdenes" var="datos" value="#{asociarColaboradoresManagedBean.ascColListHelper.listaOrdenes}"
                                 editable="false" emptyMessage="#{msj['label.lista.vacia']}" paginator="true" rows="15"
                                 paginatorTemplate="Página: {CurrentPageReport} {FirstPageLink}
                                 {PreviousPageLink}   Ir a página: {JumpToPageDropdown}   {NextPageLink} {LastPageLink}"
                                 currentPageReportTemplate="({currentPage} de {totalPages})" paginatorPosition="bottom">
                        <p:column headerText="Detalle" width="7%" >
                            <p:column>
                                <p:rowToggler/>
                            </p:column>
                        </p:column>                        
                        <p:column headerText="Número de orden">
                            <p:commandLink value="#{datos.numeroOrden}" disabled="#{datos.mostrarNumeroOrden}"
                                           action="#{asociarColaboradoresManagedBean.cargaOrdenSeleccionada}"
                                           ajax="true" update="@form" style="color:blue;" immediate="true">
                                <f:setPropertyActionListener value="#{datos}"
                                                             target="#{asociarColaboradoresManagedBean.ascColDTOHelper.ordenSeleccionado}"/>
                            </p:commandLink>
                        </p:column>
                        <p:column headerText="ID ORDEN">
                            <h:outputText value="#{datos.idOrden}"/>
                        </p:column>
                        <p:column headerText="Nombre método">
                            <h:outputText value="#{datos.feceaMetodo.nombre}"/>
                        </p:column>
                        <p:column headerText="#{msj['titulo.tabla.fecha.not']}">
                            <h:outputText value="#{datos.fecetDetalleNyV.fecNotificacionNyV}">
                                <f:convertDateTime pattern="dd/MM/yyyy"/>
                            </h:outputText>
                        </p:column>                      


                        
                         <p:rowExpansion>
                            <h:panelGrid id="display" columns="2" cellpadding="4" style="width:auto;"
                                         styleClass=" ui-widget-content grid">                                
                                <h:outputText value="Estatus :"/>
                                <h:outputText value="#{datos.fececEstatus.descripcion}"/>
                                <h:outputText value="Expediente de Orden :"/>
                                <h:commandButton ajax="true" image="/resources/images/document.png"                                 			
                                            style="padding:0px 5px;" update="docDialog"
											 action="#{asociarColaboradoresManagedBean.cargaDocumentosOrdenOficio}">   
								<f:setPropertyActionListener value="#{datos}"
                                                                  target="#{asociarColaboradoresManagedBean.ascColDTOHelper.ordenSeleccionDescarga}"/>										 
                                </h:commandButton>                                
                            </h:panelGrid>
                        </p:rowExpansion>
                    </p:dataTable>
                    <br/>
                    <br/>
                    <h:panelGrid border="0" columns="4">
                        <h:outputLabel value="Compulsas con Terceros asignadas" style="float:right;font-weight:bold"/>
                        <p:spacer width="500" height="1"/>
                    </h:panelGrid>
                    <p:separator/>
                    <p:dataTable id="tablaCompulsas" var="datos" value="#{asociarColaboradoresManagedBean.ascColListHelper.listaCompulsas}"
                                 editable="false" emptyMessage="#{msj['label.lista.vacia']}" paginator="true" rows="15"
                                 paginatorTemplate="Página: {CurrentPageReport} {FirstPageLink}
                                 {PreviousPageLink}   Ir a página: {JumpToPageDropdown}   {NextPageLink} {LastPageLink}"
                                 currentPageReportTemplate="({currentPage} de {totalPages})" paginatorPosition="bottom">
                        <p:column headerText="Detalle" width="7%" >
                            <p:column>
                                <p:rowToggler/>
                            </p:column>
                        </p:column>  
                        <p:column headerText="Número de Órden Compulsa">
                            <p:commandLink value="#{datos.orden.numeroOrden}" disabled="#{datos.mostrarNumeroOrden}"
                                           action="#{asociarColaboradoresManagedBean.cargaCompulsaSeleccionada}"
                                           ajax="true" update="@form" style="color:blue;" immediate="true">
                                <f:setPropertyActionListener value="#{datos}"
                                                             target="#{asociarColaboradoresManagedBean.ascColDTOHelper.compulsaSeleccionada}"/>
                            </p:commandLink>
                        </p:column>
                        <p:column headerText="ID COMPULSA">
                            <h:outputText value="#{datos.idCompulsa}"/>
                        </p:column>                        
                        <p:column headerText="#{msj['titulo.tabla.fecha.not']}">
                            <h:outputText value="#{datos.orden.fecetDetalleNyV.fecNotificacionContNyV}">
                                <f:convertDateTime pattern="dd/MM/yyyy"/>
                            </h:outputText>
                        </p:column>


                        <p:rowExpansion>
                            <h:panelGrid id="displayCompulsas" columns="2" cellpadding="4" style="width:auto;"
                                         styleClass=" ui-widget-content grid">                                
                                <h:outputText value="Estatus :"/>
                                <h:outputText value="#{datos.orden.fececEstatus.descripcion}"/>
                                <h:outputText value="Oficio de compulsas :"/>
                                <h:commandButton ajax="true" image="/resources/images/document.png" 
                                                 style="padding:0px 5px;">
                                    <f:setPropertyActionListener value="#{datos}"
                                                                 target="#{asociarColaboradoresManagedBean.ascColDTOHelper.compulsaSeleccionada}"/>
                                    <p:fileDownload contentDisposition="attachment"
                                                    value="#{asociarColaboradoresManagedBean.archivoDescargaOficioCompulsa}"/>
                                </h:commandButton>
                            </h:panelGrid>
                        </p:rowExpansion>

                    </p:dataTable>
                    <br/>
                            <p:commandButton value="#{msj['label.boton.pantalla.anterior']}" action="#{asociarColaboradoresManagedBean.regresaTest}" style="float:right;width:130px;" ajax="true" immediate="true" />
                       <br/>
                       <br/>
                </p:panel>
                <p:panel id="panelAsociarColaboradoresOrden"
                         style="border-bottom-right-radius:0;border-bottom-left-radius:0;" styleClass="panel-principal"
                         header="Detalle Orden"
                         rendered="#{asociarColaboradoresManagedBean.ascColAttribHelper.mostrarPanelOrdenAsociarColaboradores}">
                    <h:panelGrid border="0" columns="5">
                        <h:outputLabel value="Número Orden: " style="float:left;font-weight:bold"/>
                        <h:outputLabel value="#{asociarColaboradoresManagedBean.ascColDTOHelper.ordenSeleccionado.numeroOrden}"
                                       style="float:left;font-weight:bold"/>
                        <p:spacer width="400" height="1"/>
                        <h:outputLabel value="Método: " style="float:right;font-weight:bold"/>
                        <h:outputLabel value="#{asociarColaboradoresManagedBean.ascColDTOHelper.ordenSeleccionado.feceaMetodo.nombre}"
                                       style="float:left;font-weight:bold"/>
                    </h:panelGrid>
                    <p:separator/>
                    <br/>
                    <br/>
                    <p:accordionPanel multiple="true">
                        <p:tab title="Datos del Contribuyente">
                            <h:panelGrid columns="3" id="panelDatosContribuyente" border="0"
                                         styleClass="panel-secundario">
                                <h:outputLabel value="RFC:" style="font-weight:bold"/>
                                <p:spacer width="50" height="1"/>
                                <h:outputLabel value="Nombre/ Razón Social:" style="font-weight:bold"/>
                                <p:inputText id="txtRfc" style="width:150px;" disabled="true"
                                             value="#{asociarColaboradoresManagedBean.ascColDTOHelper.contribuyente.rfc}"/>
                                <p:spacer width="50" height="1"/>
                                <p:inputTextarea id="txtNombre" style="width:325px;height:auto;" disabled="true"
                                                 value="#{asociarColaboradoresManagedBean.ascColDTOHelper.contribuyente.nombre}"/>
                                <p:message id="msgRFC" for="txtRfc" display="text"/>
                                <p:spacer width="50" height="1"/>
                                <h:outputLabel value=" " style="font-weight:bold"/>
                                <h:outputLabel value="Régimen:" style="font-weight:bold"/>
                                <p:spacer width="50" height="1"/>
                                <h:outputLabel value="Situación del Contribuyente:" style="font-weight:bold"/>
                                <p:inputText id="txtRegimen" style="width:325px;height:auto;" disabled="true"
                                             value="#{asociarColaboradoresManagedBean.ascColDTOHelper.contribuyente.regimen}"/>
                                <p:spacer width="50" height="1"/>
                                <p:inputText id="txtSituacion" style="width:325px;height:auto;" disabled="true"
                                             value="#{asociarColaboradoresManagedBean.ascColDTOHelper.contribuyente.situacion}"/>
                                <h:outputLabel value="Tipo de Contribuyente:" style="font-weight:bold"/>
                                <p:spacer width="50" height="1"/>
                                <h:outputLabel value="Actividad Preponderante:" style="font-weight:bold"/>
                                <p:inputText id="txtTipo" style="width:325px;height:auto;" disabled="true"
                                             value="#{asociarColaboradoresManagedBean.ascColDTOHelper.contribuyente.tipo}"/>
                                <p:spacer width="50" height="1"/>
                                <p:inputTextarea id="txtActividad" style="width:325px;height:auto;" disabled="true"
                                                 value="#{asociarColaboradoresManagedBean.ascColDTOHelper.contribuyente.actividadPreponderante}"/>
                                <h:outputLabel value="Domicilio Fiscal del Contribuyente:" style="font-weight:bold"/>
                                <p:spacer width="50" height="1"/>
                                <h:outputLabel value="Situación del Domicilio:" style="font-weight:bold"/>
                                <p:inputTextarea id="txtDomFiscal" style="width:325px;height:auto;" disabled="true"
                                                 value="#{asociarColaboradoresManagedBean.ascColDTOHelper.contribuyente.domicilioFiscal}"/>
                                <p:spacer width="50" height="1"/>
                                <p:inputText id="txtDomicilio" style="width:325px;height:auto;" disabled="true"
                                             value="#{asociarColaboradoresManagedBean.ascColDTOHelper.contribuyente.situacionDomicilio}"/>
                                <h:outputLabel value="Entidad:" style="font-weight:bold"/>
                                <p:spacer width="50" height="1"/>
                                <h:outputLabel value="Medios de Contacto:" style="font-weight:bold"/>
                                <p:inputText id="txtEntidad" style="width:325px;height:auto;" disabled="true"
                                             value="#{asociarColaboradoresManagedBean.ascColDTOHelper.contribuyente.entidad}"/>
                                <p:spacer width="50" height="1"/>
                                <p:inputText id="txtMediosCon" style="width:325px;height:auto;" disabled="true"
                                             value="#{asociarColaboradoresManagedBean.ascColAttribHelper.leyendaContribuyenteMedios}"/>
                            </h:panelGrid>
                        </p:tab>
                    </p:accordionPanel>
                    <br/>
                    <br/>
                    <p:accordionPanel multiple="true">
                        <p:tab title="Agregar o Modificar Representante Legal" rendered="#{asociarColaboradoresManagedBean.ascColAttribHelper.mostrarDatosRepresentanteLegal}">
							<h:panelGrid columns="2">                            
                            	<h:panelGrid columns="2" id="panelDatosRepresentanteLegal" border="0"
                                         styleClass="panel-secundario" style="width:500px;">
                            		<h:outputLabel value="Seleccione un RFC Asociado:" style="font-weight:bold"/>
                                	<p:selectOneMenu id="labelRFCRL"
                                                 disabled="#{asociarColaboradoresManagedBean.ascColDTOHelper.representanteLegalVO.deshabilitarComboColaborador}"
                                                 value="#{asociarColaboradoresManagedBean.ascColAttribHelper.rfcRLLista}"
                                                 style="width:160px;">
                                    	<f:selectItem itemLabel="#{msj['combo.item.selecciona.rfc']}" itemValue="-1"/>
                                    	<f:selectItems value="#{asociarColaboradoresManagedBean.ascColDTOHelper.contribuyente.repLegal}"
                                                   var="lista" style="font:red;" itemLabel="#{lista.rfc}"
                                                   itemValue="#{lista.rfc}"/>
                                    	<p:ajax update="@form" event="change"
                                            listener="#{asociarColaboradoresManagedBean.llenaCamposRL}"/>
                                	</p:selectOneMenu>
                            		<h:outputLabel value="RFC:" style="font-weight:bold"/>
                                	<p:inputText style="width:200px;" id="txtRLRFC"
                                             disabled="#{asociarColaboradoresManagedBean.ascColDTOHelper.representanteLegalVO.deshabilitarCampos}"
                                             value="#{asociarColaboradoresManagedBean.ascColDTOHelper.representanteLegalVO.rfc}">
                                	</p:inputText>
                                	<h:outputLabel value="Nombre:" style="font-weight:bold"/>
                                	<p:inputText style="width:200px;"
                                             value="#{asociarColaboradoresManagedBean.ascColDTOHelper.representanteLegalVO.nombre}"
                                             disabled="#{asociarColaboradoresManagedBean.ascColDTOHelper.representanteLegalVO.deshabilitarCampos}">
                                    <p:ajax />
                                    </p:inputText>
                            		<h:outputLabel value="#{msj['label.email']}:" style="font-weight:bold"/>
                                	<p:inputText style="width:200px;"
                                             disabled="#{asociarColaboradoresManagedBean.ascColDTOHelper.representanteLegalVO.deshabilitarCorreo}"
                                             value="#{asociarColaboradoresManagedBean.ascColDTOHelper.representanteLegalVO.correo}" >
                                             
                                      <p:ajax />
                                    </p:inputText>
	                            	<h:outputLabel value="#{msj['label.medios.contacto']}" style="font-weight:bold"/>
	                                <p:selectBooleanCheckbox value="#{asociarColaboradoresManagedBean.ascColDTOHelper.representanteLegalVO.medioContactoBoolean}"
	                                                         disabled="true"/>
	                                <p:spacer width="50" height="1"/>
	                                <p:spacer width="50" height="1"/>
	                                <h:outputLabel value="Órden sin Representante L.:" style="font-weight:bold"/>
	                                <p:selectBooleanCheckbox value="#{asociarColaboradoresManagedBean.ascColDTOHelper.representanteLegalVO.sinColaborador}" 
									disabled="#{asociarColaboradoresManagedBean.ascColAttribHelper.mostrarOrdenSinRL}">
	                                    <p:ajax update="@form"
	                                            listener="#{asociarColaboradoresManagedBean.validaSinRepresentanteLegal}"/>
                                	</p:selectBooleanCheckbox>
                                	
                                	<p:spacer width="50" height="1"/>
                            	</h:panelGrid>
                            	
                            	
                            	<h:panelGrid columns="1" id="panelDatosRepresentanteLegalDocumentos" border="0"
                                         >
                            		<p:commandButton value="#{msj['consultar.btn.buscar']} " styleClass="ui-confirmdialog-no" style="float:left;"
                                                 disabled="#{asociarColaboradoresManagedBean.ascColDTOHelper.representanteLegalVO.deshabilitarBotonBusquedaColaborador}"
                                                 action="#{asociarColaboradoresManagedBean.buscarColaborador}"
                                                 update="@form" 
                                                 rendered="#{asociarColaboradoresManagedBean.ascColDTOHelper.representanteLegalVO.visibleBusquedaColaborador}">
                                    	<f:setPropertyActionListener target="#{asociarColaboradoresManagedBean.ascColAttribHelper.tipoColaborador}"
                                                                 value="RL"/>
                                	</p:commandButton>
                                	<p:commandButton value="#{msj['btn.nuevo.busqueda']} " styleClass="ui-confirmdialog-no"
                                                 action="#{asociarColaboradoresManagedBean.nuevaBusquedaColaborador}"
                                                 disabled="#{asociarColaboradoresManagedBean.ascColDTOHelper.representanteLegalVO.deshabilitarBotonBusquedaColaborador}"
                                                 update="@form" 
                                                 rendered="#{!asociarColaboradoresManagedBean.ascColDTOHelper.representanteLegalVO.visibleBusquedaColaborador}"
                                                 style="float:left;">
                                    	<f:setPropertyActionListener target="#{asociarColaboradoresManagedBean.ascColAttribHelper.tipoColaborador}"
                                                                 value="RL"/>
                                	</p:commandButton>                            	
                            		<p:spacer width="100" height="1"/>
                            		
                            		<p:commandButton value="Cancelar Busqueda" action="#{asociarColaboradoresManagedBean.cancelarNuevoColaborador}"
                                             update="@form" 
                                       style="float:left;" rendered="#{asociarColaboradoresManagedBean.ascColAttribHelper.mostrarCancelarBusquedaRL}">
                                     <f:setPropertyActionListener target="#{asociarColaboradoresManagedBean.ascColAttribHelper.tipoColaborador}"
                                                             value="RL"/>
                            		</p:commandButton>
                            		<p:spacer width="100" height="1"/>
                            		
                            		<p:commandButton value="Adjuntar documentos" onclick="PF('dlgDocRepLegal').show();"
                                     type="button"  style="float:left;" update=":formAsociar:dlgDocRepLegal" rendered="#{asociarColaboradoresManagedBean.ascColAttribHelper.mostrarAdjuntarDocumentoRL}" />
                                     
                            		<p:commandButton value="Ver documentos" onclick="PF('dlgDocRepLegal').show();"
                                     type="button"  style="float:left;" update=":formAsociar:dlgDocRepLegal" rendered="#{!asociarColaboradoresManagedBean.ascColAttribHelper.mostrarAdjuntarDocumentoRL}" />
                                     <p:spacer width="100" height="1"/>                                      
                                     <p:commandButton value="#{msj['btn.guardar']}" styleClass="ui-confirmdialog-no" update="@form"
                                                 action="#{asociarColaboradoresManagedBean.preGuardarColaborador}"
                                                 rendered="#{asociarColaboradoresManagedBean.ascColAttribHelper.mostrarGuardarRepresentanteLegal}"
                                                 style="float:left;">
                                    	<f:setPropertyActionListener target="#{asociarColaboradoresManagedBean.ascColAttribHelper.tipoColaborador}"
                                                                 value="RL"/>
                                	</p:commandButton>
                            	</h:panelGrid>
                            
                            </h:panelGrid>
                        </p:tab>
                        <p:tab title="Agregar o Modificar Apoderado Legal del Representante Legal" rendered="#{asociarColaboradoresManagedBean.ascColAttribHelper.mostrarDatosApoderadoLegalRL}">
                            <h:panelGrid columns="2">  
                            <h:panelGrid columns="2" id="panelDatosALRL" border="0" styleClass="panel-secundario">                                
                                <h:outputLabel value="RFC:" style="font-weight:bold"/>
                                <p:inputText style="width:300px;" id="txtALRLRFC"
                                             value="#{asociarColaboradoresManagedBean.ascColDTOHelper.apoderadoLegalRLVO.rfc}"
