				<h:outputText value="#{msj['consultar.label.rfc.contribuyente']}:" />
				<h:outputText
					value="#{consultarReimprimirDocumentosMB.reimprimirDocumentosDTOHelper.contribuyente.rfc}" />
				<h:outputText value="#{msj['consultar.label.razon']}:" />
