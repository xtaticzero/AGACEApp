		<p:messages id="msgExitoGuardar" showDetail="true" globalOnly="true" closable="true" />

		<h:panelGrid id="pnlConsultarDocumentosBusqueda" rendered="#{consultarReimprimirDocumentosMB.visiblePnlConsultarDocBusqueda}">
			<f:facet name="header">
