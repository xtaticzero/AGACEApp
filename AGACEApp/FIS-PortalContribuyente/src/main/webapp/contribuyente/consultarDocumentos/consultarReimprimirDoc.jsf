								value="#{oficioConsulta}" />	
								
							<p:fileDownload contentDisposition="attachment"
								value="#{consultarReimprimirDocumentosMB.descargarExpedienteOficio}" />
						</h:commandButton>
					</p:column>
					
					
					
					
					
					
					
					
					<p:column headerText="#{msj['consultar.tableoficio.acuse']}">
						<h:commandButton ajax="true"
							image="/resources/images/document.png" process="@this"
							title="#{oficioConsulta.nombreAcuseNyv}">
							<f:setPropertyActionListener
								target="#{consultarReimprimirDocumentosMB.reimprimirDocumentosDTOHelper.rutaArchivo}"
								value="#{oficioConsulta.rutaAcuseNyv}" />
							<f:setPropertyActionListener
								target="#{consultarReimprimirDocumentosMB.reimprimirDocumentosDTOHelper.nombreArchivo}"
								value="#{oficioConsulta.nombreAcuseNyv}" />
