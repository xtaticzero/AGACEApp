					<p:column headerText="#{msj['column.label.oficios.dependientes']}" style="width:70px">
                            <div align="center">
                                <p:commandLink action="#{consultarReimprimirDocumentosMB.buscarOficiosDependientes}"
                                               value="#{oficioConsulta.oficiosDependientes}" style="color:#0170b7;"
                                               update=":consultarReimprimirDoc:tablaDlgOficiosDependientes"
                                               ajax="true"  immediate="true" onclick="PF('dlgDocDependiente').show();">
                                    <f:setPropertyActionListener value="#{oficioConsulta}"
                                                                 target="#{consultarReimprimirDocumentosMB.reimprimirDocumentosDTOHelper.oficioConsultaSeleccionado}"/>
                                </p:commandLink>
                            </div>
                        </p:column> 
				</p:dataTable>
		</p:fieldset>
		<p:spacer width="495" height="1"/>
