				<h:outputText value="#{msj['consultar.label.idregistro']}:" />
				<h:outputText
					value="#{consultarReimprimirDocumentosMB.reimprimirDocumentosDTOHelper.ordenConsultaSeleccionada.idRegistro}" />
				<h:outputText value="#{msj['consultar.label.metodo']}:" />
				<h:outputText
					value="#{consultarReimprimirDocumentosMB.reimprimirDocumentosDTOHelper.ordenConsultaSeleccionada.metodo}" />
				<h:outputText value="#{msj['consultar.label.numeroorden']}:" />
				<h:outputText
					value="#{consultarReimprimirDocumentosMB.reimprimirDocumentosDTOHelper.ordenConsultaSeleccionada.numeroOrden}" />
				<h:outputText />
			</h:panelGrid>
		</h:panelGrid>
		<p:spacer height="40px"/>
		
