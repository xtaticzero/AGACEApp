                                </p:commandLink>
                            </p:outputPanel>
                        </p:column>					
					
					
					<p:column headerText="#{msj['consultar.tableoficio.tipo']}">
						<h:outputText value="#{oficioConsulta.tipoOficio}" />
					</p:column>
