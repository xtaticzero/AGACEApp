                                                update="@this,tablaSolicitudContrRechazada"/>
                                        <f:param value="#{solicitudContrRechazada.nombreArchivo}" name="paramDocumento"/>
                                    </p:commandLink>
                                </p:column>
                            </p:dataTable>

                            <p:messages id="messagesSolicitudContrRechazada" autoUpdate="false" showDetail="false"
                                        closable="true"/>

                            <p:outputLabel value="Motivo de Rechazo:" style="float:left;font-weight:bold;"/><br/><br/>

