<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<f:view xmlns:f="http://java.sun.com/jsf/core" xmlns:h="http://java.sun.com/jsf/html">
    <html xmlns="http://www.w3.org/1999/xhtml" xmlns:p="http://primefaces.org/ui">
        <f:metadata>
            <f:event type="preRenderView" listener="#{documentacionFirmanteMB.cargaPromocion}"/>
            <f:event type="preRenderView" listener="#{documentacionFirmanteMB.cargaOficiosFirmante}"/>
            <f:event type="preRenderView" listener="#{documentacionFirmanteMB.cargaSolicitudContribuyente}"/>
            <f:event type="preRenderView" listener="#{documentacionFirmanteMB.validaMetodo}"/>
            <f:event type="preRenderView" listener="#{documentacionFirmanteMB.cargarColaboradores}"/>
        </f:metadata>
        <h:head></h:head>
        <h:body>
            <h:form id="documentacionFirmanteMB" styleClass="panel-principal">
                <p:messages id="messagesOrdenes" showDetail="false" closable="true" globalOnly="true"/>
                <p:ajaxStatus onstart="PF('statusDialogFirmaDocs').show();"
                              onsuccess="PF('statusDialogFirmaDocs').hide();"/>
                <p:dialog modal="true" widgetVar="statusDialogFirmaDocs" header="#{msj['titulo.panel.espere']}"
                          draggable="true" closable="false" resizable="false">
                    <p:graphicImage value="/resources/images/ajaxloadingbar.gif"/>
                </p:dialog>
                <p:panel header="Documentaci&oacute;n de la orden" style ="width:840px" styleClass="panel-interno">
                    <h:panelGrid columns="3" styleClass="ui-widget-content grid" cellpadding="10">
                        <p:column>
                            <p:outputLabel value="N&uacute;mero de orden: " style="font-weight:bold"/>
                            <p:outputLabel value="#{firmanteSeguimientoMB.ordenSeleccionada.numeroOrden}"/>
                            <p></p>
                            <p:outputLabel value="Id de registro: " style="font-weight:bold"/>
                            <p:outputLabel value="#{firmanteSeguimientoMB.ordenSeleccionada.idRegistroPropuesta}"/>
                        </p:column>
                        <p:column>
                            <p:outputLabel value="M&eacute;todo: " style="font-weight:bold"/>
                            <p:outputLabel value="#{firmanteSeguimientoMB.ordenSeleccionada.feceaMetodo.nombre}"/>
                            <p></p>
                            <p:outputLabel value="Plazo para Concluir la Revisi&oacute;n: " style="font-weight:bold"/>
                            <p:outputLabel value="#{firmanteSeguimientoMB.ordenSeleccionada.descripcionPlazoRestante}"/>
                        </p:column>
                        <p:column>
                            <p:outputLabel value="RFC Contribuyente: " style="font-weight:bold"/>
                            <p:outputLabel value="#{firmanteSeguimientoMB.ordenSeleccionada.fecetContribuyente.rfc}"/>
                            <p></p>
                            <p:outputLabel value="Nombre/Razon Social : " style="font-weight:bold"/>
                            <p:outputLabel value="#{firmanteSeguimientoMB.ordenSeleccionada.fecetContribuyente.nombre}"/>
                        </p:column>
                    </h:panelGrid>
                    <p:separator/>
                    <p:accordionPanel multiple="true">
                        <p:tab title="Apoderado Legal">
                            <p:panelGrid styleClass="noBorders" columns="2" style="width:100%" >
                                <p:panelGrid columns="2" id="panelDatosApoderadoLegal" styleClass="noBorders" style="width:500px;" >
                                    <h:outputLabel value="RFC:" style="font-weight:bold"/>
                                    <p:inputText style="width:250px;" id="txtALRFC"
                                                 disabled="true"
                                                 value="#{documentacionFirmanteMB.colaboradoresDTO.apoderadoLegal.rfc}"/>
                                    <h:outputLabel value="Nombre/Raz&oacute;n Social:" style="font-weight:bold"/>
                                    <p:inputTextarea style="width:250px; height:auto;"
                                                     value="#{documentacionFirmanteMB.colaboradoresDTO.apoderadoLegal.nombre}"
                                                     disabled="true"/>
                                    <h:outputLabel value="#{msj['label.email']}:" style="font-weight:bold"/>
                                    <p:inputText style="width:250px;"
                                                 disabled="true"
                                                 value="#{documentacionFirmanteMB.colaboradoresDTO.apoderadoLegal.correo}"/>
                                    <h:outputLabel value="#{msj['label.medios.contacto']}:" style="font-weight:bold"/>
                                    <p:selectBooleanCheckbox value="#{documentacionFirmanteMB.colaboradoresDTO.apoderadoLegal.medioContactoBoolean}"
                                                             disabled="true"/>
                                    <h:outputLabel value="&Oacute;rden sin Apoderado Legal:" style="font-weight:bold"/>
                                    <p:selectBooleanCheckbox value="#{documentacionFirmanteMB.colaboradoresDTO.apoderadoLegal.sinColaborador}" disabled="true" />
                                </p:panelGrid>

                                <p:panelGrid columns="1" id="panelDatosApoderadoDocumentos" border="0" styleClass="noBorders">
                                    <p:spacer width="100" height="1"/>                            		
                                    <p:commandButton value="Documentos de Apoderado Legal" onclick="PF('dlgDocApoderadoLegal').show();"
                                                     type="button"  style="float:left;" />

                                </p:panelGrid>
                            </p:panelGrid>
                        </p:tab>
                        <!-- Empieza representante legal -->
                        <p:tab title="Representante Legal">
                            <p:panelGrid styleClass="noBorders" columns="2" style="width:100%" >
                                <p:panelGrid columns="2" id="panelDatosRepresentanteLegal" styleClass="noBorders" style="width:500px;" >
                                    <h:outputLabel value="RFC:" style="font-weight:bold"/>
                                    <p:inputText style="width:250px;" id="txtRLRFC"
                                                 disabled="true"
                                                 value="#{documentacionFirmanteMB.colaboradoresDTO.representanteLegal.rfc}"/>
                                    <h:outputLabel value="Nombre/Raz&oacute;n Social:" style="font-weight:bold"/>
                                    <p:inputTextarea style="width:250px; height:auto;"
                                                     value="#{documentacionFirmanteMB.colaboradoresDTO.representanteLegal.nombre}"
                                                     disabled="true"/>
                                    <h:outputLabel value="#{msj['label.email']}:" style="font-weight:bold"/>
                                    <p:inputText style="width:250px;"
                                                 disabled="true"
                                                 value="#{documentacionFirmanteMB.colaboradoresDTO.representanteLegal.correo}"/>
                                    <h:outputLabel value="#{msj['label.medios.contacto']}:" style="font-weight:bold"/>
                                    <p:selectBooleanCheckbox value="#{documentacionFirmanteMB.colaboradoresDTO.representanteLegal.medioContactoBoolean}"
                                                             disabled="true"/>
                                    <h:outputLabel value="&Oacute;rden sin Representante Legal:" style="font-weight:bold"/>
                                    <p:selectBooleanCheckbox value="#{documentacionFirmanteMB.colaboradoresDTO.representanteLegal.sinColaborador}" disabled="true" />
                                </p:panelGrid>

                                <p:panelGrid columns="1" id="panelDatosRepresentanteDocumentos" border="0" styleClass="noBorders">
                                    <p:spacer width="100" height="1"/>                            		
                                    <p:commandButton value="Documentos de Representante Legal" onclick="PF('dlgDocRepLegal').show();"
                                                     type="button"  style="float:left;" />

                                </p:panelGrid>
                            </p:panelGrid>
                        </p:tab>
                        <!-- Termina representante legal -->
                        <p:tab title="Apoderado Legal del Representante Legal">
                            <p:panelGrid styleClass="noBorders" columns="2" style="width:100%" >
                                <p:panelGrid columns="2" id="panelDatosALRL" styleClass="noBorders" style="width:500px;" >
                                    <h:outputLabel value="RFC:" style="font-weight:bold"/>
                                    <p:inputText style="width:250px;" id="txtALRLRFC"
                                                 disabled="true"
                                                 value="#{documentacionFirmanteMB.colaboradoresDTO.apoderadoLegalRepresentanteLegal.rfc}"/>
                                    <h:outputLabel value="Nombre/Raz&oacute;n Social:" style="font-weight:bold"/>
                                    <p:inputTextarea style="width:250px; height:auto;"
                                                     value="#{documentacionFirmanteMB.colaboradoresDTO.apoderadoLegalRepresentanteLegal.nombre}"
                                                     disabled="true"/>
                                    <h:outputLabel value="#{msj['label.email']}:" style="font-weight:bold"/>
                                    <p:inputText style="width:250px;"
                                                 disabled="true"
                                                 value="#{documentacionFirmanteMB.colaboradoresDTO.apoderadoLegalRepresentanteLegal.correo}"/>
                                    <h:outputLabel value="#{msj['label.medios.contacto']}:" style="font-weight:bold"/>
                                    <p:selectBooleanCheckbox value="#{documentacionFirmanteMB.colaboradoresDTO.apoderadoLegalRepresentanteLegal.medioContactoBoolean}"
                                                             disabled="true"/>
                                    <h:outputLabel value="&Oacute;rden sin Apoderado L. R.L.:" style="font-weight:bold"/>
                                    <p:selectBooleanCheckbox value="#{documentacionFirmanteMB.colaboradoresDTO.apoderadoLegalRepresentanteLegal.sinColaborador}" disabled="true" />
                                </p:panelGrid>

                                <p:panelGrid columns="1" id="panelDatosALRLDocumentos" border="0" styleClass="noBorders">
                                    <p:spacer width="100" height="1"/>                            		
                                    <p:commandButton value="Documentos de Apoderado Legal del Representante Legal" onclick="PF('dlgDocALRL').show();"
                                                     type="button"  style="float:left;" />

                                </p:panelGrid>
                            </p:panelGrid>
                        </p:tab>


                        <!-- Empieza agente aduanal -->
                        <p:tab title="Agente Aduanal">
                            <p:panelGrid styleClass="noBorders" style="width:100%">
                                <p:row>
                                    <p:column style="width:55%">
                                        <p:panelGrid id="panelDatosAA">
                                            <p:row style="width:100%">
                                                <p:column style="width:30%">
                                                    <h:outputLabel value="RFC:" style="font-weight:bold"/>
                                                </p:column>
                                                <p:column style="width:70%">
                                                    <p:inputText style="width:90%;"
                                                                 disabled="true"
                                                                 value="#{documentacionFirmanteMB.colaboradoresDTO.agenteAduanal.rfc}"/>
                                                </p:column>
                                            </p:row>
                                            <p:row style="width:100%">
                                                <p:column style="width:30%">
                                                    <h:outputLabel value="Nombre/Raz&oacute;n Social:" style="font-weight:bold"/>
                                                </p:column>
                                                <p:column style="width:70%">
                                                    <p:inputTextarea style="width:90%; height:auto;"
                                                                     value="#{documentacionFirmanteMB.colaboradoresDTO.agenteAduanal.nombre}"
                                                                     disabled="true"/>
                                                </p:column>
                                            </p:row>
                                            <p:row style="width:100%">
                                                <p:column style="width:30%">
                                                    <h:outputLabel value="#{msj['label.email']}:" style="font-weight:bold"/>
                                                </p:column>
                                                <p:column style="width:70%">
                                                    <p:inputText style="width:90%;"
                                                                 disabled="true"
                                                                 value="#{documentacionFirmanteMB.colaboradoresDTO.agenteAduanal.correo}"/>
                                                </p:column>
                                            </p:row>
                                            <p:row style="width:100%; border:0px !important; border-style:none !important">
                                                <p:column style="width:30%; border:0px !important; border-style:none !important">
                                                    <h:outputLabel value="#{msj['label.medios.contacto']}:" style="font-weight:bold"/>
                                                </p:column>
                                                <p:column style="width:70%; border:0px !important; border-style:none !important">
                                                    <p:selectBooleanCheckbox value="#{documentacionFirmanteMB.colaboradoresDTO.agenteAduanal.medioContactoBoolean}"
                                                                             disabled="true"/>
                                                </p:column>
                                            </p:row>
                                            <p:row style="width:100%;border:0px !important; border-style:none !important">
                                                <p:column style="width:30%;border:0px !important; border-style:none !important">
                                                    <h:outputLabel value="Orden sin Agente Aduanal:" style="font-weight:bold"/>
                                                </p:column>
                                                <p:column style="width:70%;border:0px !important; border-style:none !important">
                                                    <p:selectBooleanCheckbox value="#{documentacionFirmanteMB.colaboradoresDTO.agenteAduanal.sinColaborador}" disabled="true" />
                                                </p:column>
                                            </p:row>
                                        </p:panelGrid>
                                    </p:column>								
                                    <p:column style="width:45%">
                                        <div style="margin: 0px auto; height:190px; width:100%; position:relative;">
                                            <div style="width:200px; position:absolute; bottom: 0; left:25%;">
                                                <p:commandButton value="Documentos Agente Aduanal" update=":documentacionFirmanteMB:tablaPromocionesAgenteAduanal" onclick="PF('dlgDocPromocionAgenteAduanal').show();" action="#{documentacionFirmanteMB.cargaDocumentosAgenteAduanal}" rendered="true"/> 

                                            </div>
                                        </div>
                                    </p:column>
                                </p:row>
                            </p:panelGrid>
                            <p:separator/>
                        </p:tab>
                    </p:accordionPanel>	
                    <p:spacer width="40" height="10"/>	

                    <div align="center">
                        <h:outputText value="Documentaci&oacute;n de la orden" style="font-weight:bold;" />
                    </div>
                    <p:spacer width="40" height="10"/>
                    <p:fieldset legend="Firmado" toggleable="true" toggleSpeed="500" collapsed="true">
                        <p:fieldset legend="Oficios Firmados" toggleable="true" toggleSpeed="500" collapsed="true">
                            <p:dataTable id="tablaOficiosFirmados" var="oficioFirmado"
                                         value="#{documentacionFirmanteMB.listaOficiosFirmados}" editable="false"
                                         emptyMessage="#{msj['label.lista.vacia']}" paginator="true" rows="5"
                                         paginatorTemplate="Página: {CurrentPageReport} {FirstPageLink}                                     {PreviousPageLink} &amp;nbsp; Ir a página: {JumpToPageDropdown} &amp;nbsp; {NextPageLink} {LastPageLink}"
                                         currentPageReportTemplate="({currentPage} de {totalPages})" paginatorPosition="bottom">
                                <p:column headerText="#{msj['column.label.detalle']}" style="width:35px">
                                    <p:column>
                                        <p:rowToggler/>
                                    </p:column>
                                </p:column>
                                <p:column headerText="#{msj['column.label.id.oficio']}" style="width:50px">
                                    <p:commandLink style="color:#0170b7;" action="documentacionOficio" disabled="#{!oficioFirmado.adminOficio.blnDocReq}">
                                        <h:outputText value="#{oficioFirmado.idOficio}" style="color:blue;"/>
                                        <f:setPropertyActionListener value="#{oficioFirmado}"
                                                                     target="#{documentacionOficioFirmanteMB.oficioSeleccionado}"/>
                                        <f:param name="idOficio" value="#{oficioFirmado.idOficio}"/>
                                    </p:commandLink>
                                </p:column>
                                <p:column headerText="#{msj['column.label.tipo.oficio']}" style="width:150px">
                                    #{oficioFirmado.fecetTipoOficio.nombre}
                                </p:column>
                                <p:column headerText="#{msj['column.label.oficio']}" style="width:50px">
                                    <div align="center">
                                        <h:commandButton image="/resources/images/document.png"
                                                         title="#{oficioFirmado.nombreArchivo}" ajax="true" update="msgOficio"
                                                         style="padding:0px 5px;">
                                            <f:setPropertyActionListener value="#{oficioFirmado}"
                                                                         target="#{documentacionFirmanteMB.oficioSeleccionado}"/>
                                            <p:fileDownload contentDisposition="attachment"
                                                            value="#{documentacionFirmanteMB.archivoDescargaDocumentacion}"/>
                                        </h:commandButton>
                                    </div>
                                </p:column>
                           <!--     <p:column headerText="#{msj['label.acuse.recepcion.sello.digital']}" style="width:110px">
                                    <div align="center">
                                        <h:commandButton image="/resources/images/document.png"
                                                         title="#{oficioFirmado.nombreAcuseNyv}" ajax="true"
                                                         style="padding:0px 5px;">
                                            <f:setPropertyActionListener value="#{oficioFirmado}"
                                                                         target="#{documentacionFirmanteMB.oficioSeleccionado}"/>
                                            <p:fileDownload contentDisposition="attachment"
                                                            value="#{documentacionFirmanteMB.archivoDescargaDocumentoAnexo}"/>
                                        </h:commandButton>
                                    </div>
                                </p:column> -->
                                <p:column headerText="#{msj['column.label.fecha.notificacion']}" style="width:70px">
                                    <h:outputLabel value="#{oficioFirmado.fecetDetalleNyV.fecNotificacionContNyV}">
                                        <f:convertDateTime pattern="dd/MM/yyyy"/>
                                    </h:outputLabel>
                                </p:column>
                                <p:column headerText="#{msj['column.label.fecha.firma']}" style="width:70px">
                                    <h:outputLabel value="#{oficioFirmado.fechaFirma}">
                                        <f:convertDateTime pattern="dd/MM/yyyy"/>
                                    </h:outputLabel>
                                </p:column>
                                <p:column headerText="#{msj['column.label.oficios.dependientes']}" style="width:60px">
                                    <p:commandLink value=" #{oficioFirmado.totalOficiosDependientes}" ajax="false"
                                                   action="#{documentacionFirmanteMB.getOficiosDependientesFirmados}"
                                                   style="color:#0170b7;">
                                        <f:setPropertyActionListener value="#{oficioFirmado}"
                                                                     target="#{documentacionFirmanteMB.oficioSeleccionado}"/>
                                    </p:commandLink>
                                </p:column>
                                <p:rowExpansion>
                                    <h:panelGrid columns="2" cellpadding="4" style="width:auto;"
                                                 styleClass=" ui-widget-content grid">
                                        <h:outputText value="#{msj['column.label.anexos.oficio']} :"/>
                                        <p:commandLink value="#{oficioFirmado.totalAnexosOficio}" style="color:#0170b7;"
                                                       ajax="false" action="#{documentacionFirmanteMB.getOficiosAnexos}">
                                            <f:setPropertyActionListener value="#{oficioFirmado}"
                                                                         target="#{documentacionFirmanteMB.oficioSeleccionado}"/>
                                        </p:commandLink>
                                        <h:outputText value="#{msj['column.label.fecha.carga.oficio']} :"/>
                                        <h:outputLabel value="#{oficioFirmado.fechaCreacion}">
                                            <f:convertDateTime pattern="dd/MM/yyyy"/>
                                        </h:outputLabel>
                                    </h:panelGrid>
                                </p:rowExpansion>
                            </p:dataTable>
                        </p:fieldset>
                        <p:spacer width="40" height="10"/>
                        <p:fieldset legend="Solicitudes de Contribuyente firmadas" toggleable="true" toggleSpeed="500" collapsed="true">

                            <p:dataTable id="tablaSolicitudContr" var="solicitudContr" rendered="true"
                                         value="#{documentacionFirmanteMB.listaSolicitudContribuyenteFirmadas}" editable="false"
                                         emptyMessage="#{msj['label.lista.vacia']}" paginator="true" rows="5"
                                         paginatorTemplate="Página: {CurrentPageReport} {FirstPageLink}                                     {PreviousPageLink} &amp;nbsp; Ir a página: {JumpToPageDropdown} &amp;nbsp; {NextPageLink} {LastPageLink}"
                                         currentPageReportTemplate="({currentPage} de {totalPages})"
                                         paginatorPosition="bottom">
                                <p:column headerText="#{msj['label.id.solicitud.contribuyente']}" style="width:40px">
                                    #{solicitudContr.id}
                                </p:column>
                                <p:column headerText="Fecha Solicitud" style="width:30px">
                                    <h:outputLabel value="#{solicitudContr.fechaCarga}">
                                        <f:convertDateTime pattern="dd/MM/yyyy"/>
                                    </h:outputLabel>
                                </p:column>
                                <p:column headerText="#{msj['label.descripcion.solicitud.contribuyente']}" style="width:80px">
                                    <h:outputLabel value="#{solicitudContr.descripcion}"/>                                   
                                </p:column>                            
                                <p:column headerText="#{msj['column.label.documentacion.contribuyente']}" style="width:50px">
                                    <div align="center">
                                        <p:commandLink action="#{documentacionFirmanteMB.getDocumentacionSolicitudContribuyente}" value="#{solicitudContr.totalDocumentosContribuyente}" style="color:#0170b7;" ajax="true" onclick="PF('dialogSolicitudContrDocContribuyente').show();" immediate="true" update=":documentacionFirmanteMB:panelDocSolicitudCont">
                                            <f:setPropertyActionListener value="#{solicitudContr}" target="#{documentacionFirmanteMB.solicitudContribuyenteVOSeleccionado}"/>
                                        </p:commandLink>
                                    </div>
                                </p:column>
                                <p:column headerText="#{msj['column.label.estatus']}" style="width:80px">
                                    <p:outputLabel value="#{solicitudContr.estatus}"/>
                                </p:column>
                                <p:column headerText="#{msj['consultar.tableprorrogaoficios.docresol']}" style="width:60px">
                                    <h:commandButton image="/resources/images/document.png"
                                                     title="#{solicitudContr.nombreResolucion}" ajax="true"
                                                     style="float:left;padding:0px 5px;">
                                        <f:setPropertyActionListener value="#{solicitudContr}"
                                                                     target="#{documentacionFirmanteMB.solicitudContribuyenteVOSeleccionado}"/>
                                        <p:fileDownload contentDisposition="attachment"
                                                        value="#{documentacionFirmanteMB.descargaDocumentoSolicitudContribuyente}"/>
                                    </h:commandButton>
                                </p:column>
                                <p:column headerText="#{msj['column.label.justificacion']}" style="width:80px">
                                    <h:outputText value="#{solicitudContr.justificacion}"/>
                                </p:column>
                                <p:column headerText="#{msj['consultar.tableprorrogaoficios.anexosresol']}"
                                          style="width:40px">
                                    <p:commandLink value="#{solicitudContr.totalDocumentosRechazo}" ajax="false"
                                                   action="#{documentacionFirmanteMB.getAnexosResolucionSolicitudContribuyente}"
                                                   style="color:#0170b7;">
                                        <f:setPropertyActionListener value="#{solicitudContr}"
                                                                     target="#{documentacionFirmanteMB.solicitudContribuyenteVOSeleccionado}"/>
                                    </p:commandLink>
                                </p:column>


                            </p:dataTable>
                        </p:fieldset>           		                    
                    </p:fieldset>
                    <p:spacer width="40" height="10"/>
                    <p:fieldset legend="Por firmar" toggleable="true" toggleSpeed="500" collapsed="true">
                        <p:fieldset legend="Oficios por firmar" toggleable="true" toggleSpeed="500" collapsed="true">
                            <p:remoteCommand name="seccionTablaOficiosPorFirmar" update="tablaDocumentosPorFirmar"/>			                    
                            <h:panelGrid columns="2" layout="grid" id="panelOficioPendiente" width="100%">
                                <p:row>
                                    <p:dataTable id="tablaDocumentosPorFirmar"
                                                 value="#{documentacionFirmanteMB.listaOficiosPorFirmar}"
                                                 var="oficioPendienteFirma" rowKey="#{oficioPendienteFirma.idOficio}"
                                                 selection="#{documentacionFirmanteMB.oficioSeleccionado}"
                                                 emptyMessage="#{msj['label.lista.vacia']}" paginator="true" rows="5"
                                                 paginatorTemplate="Página: {CurrentPageReport} {FirstPageLink} {PreviousPageLink} &amp;nbsp; Ir a página: {JumpToPageDropdown} &amp;nbsp; {NextPageLink} {LastPageLink}"
                                                 currentPageReportTemplate="({currentPage} de {totalPages})"
                                                 paginatorPosition="bottom">
                                        <p:column selectionMode="single" style="width:6px;text-align:center"/>
                                        <p:ajax event="rowSelectRadio" 
                                                update=":documentacionFirmanteMB:btnFirmaOficio :documentacionFirmanteMB:btnMotivoRechazoOfiico"
                                                listener="#{documentacionFirmanteMB.validaSelectRadioOficio}"/>
                                        <p:column headerText="#{msj['column.label.id.oficio']}" style="width:30px">
                                            <p:commandLink id="ajax1" action="detalleOficioCompulsa.jsf" style="color:#0170b7;"
                                                           value="#{oficioPendienteFirma.idOficio}">
                                                <f:param name="identificador1"
                                                         value="#{oficioPendienteFirma.rfcCompulsado}"/>
                                                <f:param name="identificador2" value="#{oficioPendienteFirma.idOficio}"/>
                                                <f:param name="identificador3"
                                                         value="#{firmanteSeguimientoMB.ordenSeleccionada.idOrden}"/>
                                                <f:param name="identificador4"
                                                         value="#{oficioPendienteFirma.fecetTipoOficio.idTipoOficio}"/>
                                                <f:param name="identificador5"
                                                         value="#{oficioPendienteFirma.fecetTipoOficio.nombre}"/>
                                            </p:commandLink>
                                        </p:column>
                                        <p:column headerText="#{msj['column.label.tipo.oficio']}" style="width:60px">
                                            #{oficioPendienteFirma.fecetTipoOficio.nombre}
                                        </p:column>
                                        <p:column headerText="#{msj['column.label.oficio']}" style="width:10px">
                                            <div align="center">
                                                <h:commandButton title="#{oficioPendienteFirma.nombreArchivo}" ajax="true" process="@this"
                                                                 image="/resources/images/document.png"
                                                                 style="padding:0px 5px;">
                                                    <f:setPropertyActionListener value="#{oficioPendienteFirma}"
                                                                                 target="#{documentacionFirmanteMB.oficioSeleccionado}"/>
