<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<f:view xmlns:f="http://java.sun.com/jsf/core" xmlns:h="http://java.sun.com/jsf/html" xmlns:ui="http://java.sun.com/jsf/facelets">
    <html xmlns="http://www.w3.org/1999/xhtml" xmlns:p="http://primefaces.org/ui">
        <f:metadata>
            <f:event type="preRenderView" listener="#{documentacionOrdenMB.init}"/>
        </f:metadata>
        <h:head/>
        <h:body>
            <h:form id="formDocumentacionOrden" styleClass="panel-principal">
                <p:ajaxStatus onstart="PF('statusDialogDocs').show();" onsuccess="PF('statusDialogDocs').hide();"/>
                <p:dialog modal="true" widgetVar="statusDialogDocs" header="#{msj['titulo.panel.espere']}" draggable="false" closable="false" resizable="false">
                    <p:graphicImage value="/resources/images/ajaxloadingbar.gif"/>
                </p:dialog>
                <p:messages id="msgExitoSubordinadosAsoc" showDetail="true" globalOnly="false" closable="true" for="msgExitoSubordinados"/>
                <p:panel id="panelDocumentacionOrden" header="Documentación de la orden" styleClass="panel-interno" style="width:100% !important; height:100% !important;">
                    <!-- Inicio de los Datos Generales de la orden -->
                    <h:panelGrid columns="3" style="width:100% !important; height:100% !important;">
                        <ui:include src="/auditor/seguimiento/componentes/datosGeneralOrden.jsf"/>
                    </h:panelGrid>
                    <!-- Fin de los Datos Generales de la orden -->
                    <p:separator/>
                    <br/>
                    <br/>                    
                    <!-- Fin de bloque de Documentacion Requerida Pruebas y Alegatos -->
                    <br/>
                    <br/>
                    
                    <p:accordionPanel multiple="true">
							<!-- Empieza apoderado legal -->
							<p:tab title="Apoderado Legal">								
								<p:panelGrid styleClass="noBorders" columns="2" style="width:100%" >
									<p:panelGrid columns="2" id="panelDatosApoderadoLegal" styleClass="noBorders" style="width:500px;" >
										<h:outputLabel value="RFC:" style="font-weight:bold"/>
										<p:inputText style="width:250px;" id="txtRLRFC"
					                                         disabled="true"
					                                         value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.apoderadoLegal.rfc}"/>
										<h:outputLabel value="Nombre/Raz&oacute;n Social:" style="font-weight:bold"/>
										<p:inputTextarea style="width:250px;"
					                                         value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.apoderadoLegal.nombre}"
					                                         disabled="true"/>
										<h:outputLabel value="#{msj['label.email']}:" style="font-weight:bold"/>
										<p:inputText style="width:250px;"
					                                         disabled="true"
					                                         value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.apoderadoLegal.correo}"/>
										<h:outputLabel value="#{msj['label.medios.contacto']}:" style="font-weight:bold"/>
										<p:selectBooleanCheckbox value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.apoderadoLegal.medioContactoBoolean}"
					                                                     disabled="true"/>
					                    <h:outputLabel value="Orden sin Representante Legal:" style="font-weight:bold" />
					                    <p:selectBooleanCheckbox value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.apoderadoLegal.sinColaborador}" disabled="true" />                             

									</p:panelGrid>
									
									<p:panelGrid columns="1" id="panelDatosApoderadoLegalDocumentos" border="0" styleClass="noBorders">
                             
		                            	<p:spacer width="100" height="1"/>                            		
		                        	 	<p:commandButton value="Documentos de Apoderado Legal" onclick="PF('dlgDocApoderadoLegal').show();"
		                                     type="button"  style="float:left;" />
		                             		                             
		                             
	                             	</p:panelGrid>
									
									
								</p:panelGrid>
		                    </p:tab>
	                    </p:accordionPanel>
	                    <br/>
                    	<br/>
                    
                     <p:accordionPanel multiple="true">
							<!-- Empieza representante legal -->
							<p:tab title="Representante Legal">								
								<p:panelGrid styleClass="noBorders" columns="2" style="width:100%" >
									<p:panelGrid columns="2" id="panelDatosAgenteLegal" styleClass="noBorders" style="width:500px;" >
										<h:outputLabel value="RFC:" style="font-weight:bold"/>
										<p:inputText style="width:250px;" id="txtRLRFC"
					                                         disabled="true"
					                                         value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.representanteLegal.rfc}"/>
										<h:outputLabel value="Nombre/Raz&oacute;n Social:" style="font-weight:bold"/>
										<p:inputTextarea style="width:250px;"
					                                         value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.representanteLegal.nombre}"
					                                         disabled="true"/>
										<h:outputLabel value="#{msj['label.email']}:" style="font-weight:bold"/>
										<p:inputText style="width:250px;"
					                                         disabled="true"
					                                         value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.representanteLegal.correo}"/>
										<h:outputLabel value="#{msj['label.medios.contacto']}:" style="font-weight:bold"/>
										<p:selectBooleanCheckbox value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.representanteLegal.medioContactoBoolean}"
					                                                     disabled="true"/>
					                    <h:outputLabel value="Orden sin Representante Legal:" style="font-weight:bold" />
					                    <p:selectBooleanCheckbox value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.representanteLegal.sinColaborador}" disabled="true" />                             

									</p:panelGrid>
									
									<p:panelGrid columns="1" id="panelDatosRepresentanteLegalDocumentos" border="0" styleClass="noBorders">
                             
		                            	<p:spacer width="100" height="1"/>                            		
		                        	 	<p:commandButton value="Documentos de Representante Legal" onclick="PF('dlgDocRepLegal').show();"
		                                     type="button"  style="float:left;" />
		                             		                             
		                             
	                             	</p:panelGrid>
									
									
								</p:panelGrid>
		                    </p:tab>
	                    </p:accordionPanel>
	                    <br/>
                    	<br/>	                    
						<!-- Termina representante legal -->
						
						
						<p:accordionPanel multiple="true">
							<!-- Empieza apoderado legal del representante legal -->
							<p:tab title="Apoderado Legal del Representante Legal">								
								<p:panelGrid styleClass="noBorders" columns="2" style="width:100%" >
									<p:panelGrid columns="2" id="panelDatosALRL" styleClass="noBorders" style="width:500px;" >
										<h:outputLabel value="RFC:" style="font-weight:bold"/>
										<p:inputText style="width:250px;" id="txtRLRFC"
					                                         disabled="true"
					                                         value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.apoderadoLegalRepresentanteLegal.rfc}"/>
										<h:outputLabel value="Nombre/Raz&oacute;n Social:" style="font-weight:bold"/>
										<p:inputTextarea style="width:250px;"
					                                         value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.apoderadoLegalRepresentanteLegal.nombre}"
					                                         disabled="true"/>
										<h:outputLabel value="#{msj['label.email']}:" style="font-weight:bold"/>
										<p:inputText style="width:250px;"
					                                         disabled="true"
					                                         value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.apoderadoLegalRepresentanteLegal.correo}"/>
										<h:outputLabel value="#{msj['label.medios.contacto']}:" style="font-weight:bold"/>
										<p:selectBooleanCheckbox value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.apoderadoLegalRepresentanteLegal.medioContactoBoolean}"
					                                                     disabled="true"/>
					                    <h:outputLabel value="Orden sin Apoderado L. R.L.:" style="font-weight:bold" />
					                    <p:selectBooleanCheckbox value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.apoderadoLegalRepresentanteLegal.sinColaborador}" disabled="true" />                             

									</p:panelGrid>
									
									<p:panelGrid columns="1" id="panelDatosALRLDocumentos" border="0" styleClass="noBorders">
                             
		                            	<p:spacer width="100" height="1"/>                            		
		                        	 	<p:commandButton value="Documentos de Apoderado Legal del Representante Legal" onclick="PF('dlgDocALRL').show();"
		                                     type="button"  style="float:left;" />
		                             		                             
		                             
	                             	</p:panelGrid>
									
									
								</p:panelGrid>
		                    </p:tab>
	                    </p:accordionPanel>
	                    <br/>
                    	<br/>	                    
						<!-- Termina representante legal -->
						
						
						<p:accordionPanel multiple="true">
							<!-- Empieza agente aduanal -->
							<p:tab title="Agente Aduanal">
								<p:panelGrid styleClass="noBorders" columns="2" style="width:100%" >
									<p:panelGrid columns="2" id="panelDatosAgenteLegal" styleClass="noBorders" style="width:500px;" >
										<h:outputLabel value="RFC:" style="font-weight:bold"/>
										<p:inputText style="width:250px;" id="txtRLRFC"
					                                         disabled="true"
					                                         value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.agenteAduanal.rfc}"/>
										<h:outputLabel value="Nombre/Raz&oacute;n Social:" style="font-weight:bold"/>
										<p:inputTextarea style="width:250px;"
					                                         value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.agenteAduanal.nombre}"
					                                         disabled="true"/>
					                    <h:outputLabel value="#{msj['label.email']}:" style="font-weight:bold"/>
										<p:inputText style="width:250px;"
					                                         disabled="true"
					                                         value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.agenteAduanal.correo}"/>
					                    <h:outputLabel value="#{msj['label.medios.contacto']}:" style="font-weight:bold"/>
										<p:selectBooleanCheckbox value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.agenteAduanal.medioContactoBoolean}"
			                                                     disabled="true"/>
			                            <h:outputLabel value="Orden sin Agente Aduanal:" style="font-weight:bold"/>
					                 	<p:selectBooleanCheckbox value="#{documentacionOrdenMB.dtoHelper.colaboradoresDTO.agenteAduanal.sinColaborador}" disabled="true" />                             					                 				                            					                                         								
									</p:panelGrid>
									
									<p:panelGrid columns="1" id="panelDatosRepresentanteLegalDocumentos" border="0" styleClass="noBorders">                             
		                            	<p:spacer width="100" height="1"/>                            		
		                        	 	<p:commandButton value="Documentos Agente Aduanal" action="#{documentacionOrdenMB.cargaDocumentosAgenteAduanal}" rendered="true"/>		                             		                             		                             
	                             	</p:panelGrid>									
								</p:panelGrid>								
		                    </p:tab>
	                    </p:accordionPanel>
                    
                    <!-- Inicio de bloque de Oficios --> 
                    <p:messages id="msgExitoGuardarDocsOficios" showDetail="true" globalOnly="false" closable="true" for="msgExitoGuardarDocsMsg"/>
    				<p:messages id="msgExitoGuardarDocs" showDetail="true" globalOnly="true" />
    				
                    <p:outputPanel id="panelOficios">                   
                    	<ui:include src="/auditor/seguimiento/componentes/tabOficios.jsf"/>
                    </p:outputPanel>
                    <!-- Fin de bloque de Oficios -->
                    <p:messages id="msgExitoGuardarReactIntegrar" showDetail="false" globalOnly="false" closable="true" for="msgExitoGuardarReactIntegrarMsg"/>
                    <p:panel id="pnlIntegracionReactivaciones" style="border:none; width:100%;padding:none;" 
                    	rendered="#{documentacionOrdenMB.attributeHelper.visualizarIntegrarExpediente ||                     		         
                    	            documentacionOrdenMB.attributeHelper.visualizarReactivarPlazoAcuerdoConclusivo}">
                        <p:separator rendered="#{documentacionOrdenMB.attributeHelper.visualizarIntegrarExpediente || documentacionOrdenMB.attributeHelper.visualizarReactivarPlazoREE || documentacionOrdenMB.attributeHelper.visualizarReactivarPlazoAcuerdoConclusivo}"/>
                        <p:outputLabel value="#{msj['titulo.panel.integracion.expediente']}" style="font-weight:bold;" rendered="#{documentacionOrdenMB.attributeHelper.visualizarIntegrarExpediente}"/>
                        <p:outputLabel value="#{msj['titulo.panel.reactivar.plazo.revision.electronica']}" style="font-weight:bold;" rendered="#{documentacionOrdenMB.attributeHelper.visualizarReactivarPlazoREE}"/>
                        <p:outputLabel value="#{msj['titulo.panel.reactivar.plazo.acuerdo.conclusivo']}" style="font-weight:bold;" rendered="#{documentacionOrdenMB.attributeHelper.visualizarReactivarPlazoAcuerdoConclusivo}"/>
                        <br/>
                        <br/>
                        <p:panel id="panelIntegrarExpediente" style="width:100%;padding:none;" columns="4" rendered="#{documentacionOrdenMB.attributeHelper.visualizarIntegrarExpediente}">
                            <p:outputLabel value="#{msj['label.fecha.integracion.expediente']}: " style="font-weight:bold;"/>
                            <h:outputLabel value="#{documentacionOrdenMB.attributeHelper.fechaActual}">
                                <f:convertDateTime pattern="dd/MM/yyyy"/>
                            </h:outputLabel>
                            <p:spacer width="10"/>
                            <p:commandButton id="btnIntegrarExpediente" value="#{msj['boton.integrar.expediente']}" action="#{documentacionOrdenMB.integrarExpediente}" style="width:120px;" immediate="true" oncomplete="ocultaComponente('btnIntegrarExpediente'); ocultaMsgAgace('msgExitoGuardarReactIntegrar');" update="@form"/>
                        </p:panel>                       
                        <p:panel id="panelReactivarPlazoAcuerdoConclusivo" style="width:100%;padding:none;" columns="4" rendered="#{documentacionOrdenMB.attributeHelper.visualizarReactivarPlazoAcuerdoConclusivo}">
                            <p:outputLabel value="#{msj['label.fecha.reactivacion']}: " style="font-weight:bold;"/>
                            <h:outputLabel value="#{documentacionOrdenMB.attributeHelper.fechaActual}">
                                <f:convertDateTime pattern="dd/MM/yyyy"/>
                            </h:outputLabel>
                            <p:spacer width="10"/>
                            <p:commandButton id="btnReactivarPlazoAcuerdoConclusivo" value="#{msj['boton.reactivar.plazo']}" action="#{documentacionOrdenMB.attributeHelper.reactivarPlazoAcuerdoConclusivo}" style="width:120px;" ajax="true" immediate="true" oncomplete="ocultaComponente('btnReactivarPlazoAcuerdoConclusivo'); ocultaMsgAgace('msgExitoGuardarReactIntegrar');" update="msgExitoGuardarReactIntegrar"/>
                        </p:panel>
                        <br/>
                    </p:panel>
                    <p:panel id="panelReactivarPlazo" style="width:100%;padding:none;" columns="4" 
                    		 tyleClass="noBorders" rendered="#{documentacionOrdenMB.attributeHelper.visualizarReactivarPlazoREE}">
                    	   <p:panelGrid width="100%" styleClass="noBorders" style="width:100%">
                    	   	<p:row>
	                           <p:column style="text-align:right" width="15%">
	                           		<p:outputLabel value="#{msj['label.fecha.reactivacion']}: " style="font-weight:bold;"/></p:column>
	                           <p:column style="text-align:right" width="15%">
		                           <h:outputLabel value="#{documentacionOrdenMB.attributeHelper.fechaReactivarPlazo}">
		                               <f:convertDateTime pattern="dd/MM/yyyy"/>
		                           </h:outputLabel>
		                       </p:column>
	                           <p:column style="text-align:left" width="15%">
	                           		<p:outputLabel value="#{msj['label.oficio.reactivacion']}: " style="font-weight:bold;"/></p:column>
	                           <p:column style="text-align:right" width="40%">
		                           <p:selectOneMenu id="console" value="#{documentacionOrdenMB.attributeHelper.idOficioReactivacion}">
							            <f:selectItem itemLabel="Selecciona el oficio" itemValue="0" />
							            <f:selectItems value="#{documentacionOrdenMB.lstHelper.listaSuspensiones}" 
							            	var="suspension" itemLabel="#{suspension.nombreOficio}" itemValue="#{suspension.idOficio}" />
							        </p:selectOneMenu>
							   </p:column>
	                         	<p:column style="text-align:left" width="15%"> 
		                           <p:commandButton id="btnReactivarPlazoREE" value="#{msj['boton.reactivar.plazo']}" style="width:120px;" 
		                           onclick="PF('confirmarReactivacion').show();" 
		                           oncomplete="ocultaMsgAgace('msgExitoGuardarReactIntegrar');" 
		                           update=":formDocumentacionOrden:panelReactivarPlazo, 
		                           		   :formDocumentacionOrden:msgExitoGuardarReactIntegrar"/>
	                           </p:column>
	                         </p:row>
                           </p:panelGrid>
                    </p:panel>
                    <br/>
                    <!-- Inicio de bloque de Oficios Firmados -->
                    <p:fieldset legend="Oficios firmados" toggleable="true" toggleSpeed="500" collapsed="true">
                    <p:fieldset legend="Oficios firmados" toggleable="true" toggleSpeed="500" collapsed="true">
                    <h:panelGrid id="panelDatoHistoricoOficiosFirmado" columns="3" border="0" style="width:100% !important; height:100% !important;">
                        <p:dataTable id="tablaHistoricoOficiosFirmados" var="documentacion" value="#{documentacionOrdenMB.lstOficioHelper.listaOficiosFirmados}" editable="false" emptyMessage="#{msj['label.lista.vacia']}" paginator="true" rows="5" paginatorTemplate="Página: {CurrentPageReport} {FirstPageLink}                                     {PreviousPageLink} &amp;nbsp; Ir a página: {JumpToPageDropdown} &amp;nbsp; {NextPageLink} {LastPageLink}" currentPageReportTemplate="({currentPage} de {totalPages})" paginatorPosition="bottom">
                            <f:facet name="header">
                                #{msj[&apos;titulo.tbl.oficios.firmados&apos;]}
                            </f:facet>
                            <!--  <p:column headerText="#{msj['column.label.detalle']}" style="width:35px">
                                <p:column>
                                    <p:rowToggler/>
                                </p:column>
                            </p:column>-->
                            <p:column headerText="#{msj['column.label.id.oficio']}" style="width:10%">
                            <div align="center">
                                <p:commandLink style="color:blue;" action="documentacionOficio" value="#{documentacion.idOficio}" disabled="#{!documentacion.adminOficio.blnDocReq}" >
                                    <f:setPropertyActionListener value="${documentacion}" target="#{documentacionOficioMB.oficioSeleccionado}"/>
                                    <f:setPropertyActionListener value="${documentacionOrdenMB.dtoHelper.ordenSeleccionada}" target="#{documentacionOficioMB.ordenSeleccionada}"/>
                                    <f:param name="identificador1" value="#{documentacionOrdenMB.dtoHelper.ordenSeleccionada.feceaMetodo.idMetodo}"/>
                                </p:commandLink>
                            </div>
                            </p:column>
                            <p:column headerText="#{msj['column.label.tipo.oficio']}" style="width:20%">
                            	<div align="center">
                                	<h:outputText value="#{documentacion.fecetTipoOficio.nombre}"/>
                                </div>
                            </p:column>
                            <p:column headerText="#{msj['column.label.oficio']}" style="width:10%">
                                <div align="center">
                                    <h:commandButton image="/resources/images/document.png" title="#{documentacion.nombreArchivo}" ajax="true" immediate="true" style="padding:0px 5px;">
                                        <f:setPropertyActionListener value="#{documentacion}" target="#{documentacionOrdenMB.dtoHelper.oficioSeleccionado}"/>
                                        <p:fileDownload contentDisposition="attachment" value="#{documentacionOrdenMB.archivoDescargaOficio}"/>
                                    </h:commandButton>
                                </div>
                            </p:column>
                            <!-- <p:column headerText="#{msj['label.acuse.recepcion.sello.digital']}" style="width:110px">
                                <div align="center">
                                    <h:commandButton image="/resources/images/document.png" title="Acuse" ajax="true" immediate="true" style="padding:0px 5px;">
                                        <f:setPropertyActionListener value="#{documentacion}" target="#{documentacionOrdenMB.dtoHelper.oficioSeleccionado}"/>
                                        <p:fileDownload contentDisposition="attachment" value="#{documentacionOrdenMB.archivoDescargaAcuse}"/>
                                    </h:commandButton>
                                </div>
                            </p:column> -->
                            <p:column headerText="#{msj['column.label.fecha.notificacion']}" style="width:15%">
                            	<div align="center">
	                                <h:outputLabel value="#{documentacion.fecetDetalleNyV.fecNotificacionNyV}">
	                                    <f:convertDateTime pattern="dd/MM/yyyy"/>
	                                </h:outputLabel>
	                            </div>
                            </p:column>
                            <p:column headerText="#{msj['column.label.fecha.firma']}" style="width:10%">
                            	<div align="center">
	                                <h:outputLabel value="#{documentacion.fechaFirma}">
	                                    <f:convertDateTime pattern="dd/MM/yyyy"/>
	                                </h:outputLabel>
	                            </div>
                            </p:column>
                            <p:column headerText="#{msj['column.label.oficios.dependientes']}" style="width:15%">
                                <div align="center">
                                    <p:commandLink action="#{documentacionOrdenMB.getOficiosDependientesEstatusOficioNotificadoContribuyente}" value="#{documentacion.totalOficiosDependientes}" style="color:#0170b7;" ajax="true" onclick="PF('dlgOficiosDependientes').show();" immediate="true" update=":formDocumentacionOrden:tablaDlgOficiosDependientes">
                                        <f:setPropertyActionListener value="#{documentacion}" target="#{documentacionOrdenMB.dtoHelper.oficioSeleccionado}"/>
                                    </p:commandLink>
                                </div>
                            </p:column>
                            <p:column headerText="#{msj['column.label.anexos.oficio']}" style="width:10%">
                                <div align="center">
                                    <p:commandLink action="#{documentacionOrdenMB.getAnexosOficio}" value="#{documentacion.totalAnexosOficio}" style="color:#0170b7;" ajax="true" onclick="PF('dlgAnexosOficio').show();" immediate="true" update=":formDocumentacionOrden:tablaDlgAnexosOficio">
                                        <f:setPropertyActionListener value="#{documentacion}" target="#{documentacionOrdenMB.dtoHelper.oficioSeleccionado}"/>
                                    </p:commandLink>
                                </div>
                            </p:column>
                            <p:column headerText="#{msj['column.label.fecha.carga.oficio']}" style="width:10%">
                                <div align="center">
	                                <h:outputLabel value="#{documentacion.fechaCreacion}">
	                                    <f:convertDateTime pattern="dd/MM/yyyy"/>
	                                </h:outputLabel>
	                            </div>
                            </p:column>
                            <!-- <p:rowExpansion>
                                <h:panelGrid columns="2" cellpadding="4" style="width:auto;" styleClass=" ui-widget-content grid">
                                    <h:outputText value="#{msj['column.label.anexos.oficio']} :"/>
                                    <p:commandLink action="#{documentacionOrdenMB.getAnexosOficio}" value="#{documentacion.totalAnexosOficio}" style="color:#0170b7;" ajax="true" onclick="PF('dlgAnexosOficio').show();" immediate="true" update=":formDocumentacionOrden:tablaDlgAnexosOficio">
                                        <f:setPropertyActionListener value="#{documentacion}" target="#{documentacionOrdenMB.dtoHelper.oficioSeleccionado}"/>
                                    </p:commandLink>
                                    <h:outputText value="#{msj['column.label.fecha.carga.oficio']} :"/>
                                    <h:outputLabel value="#{documentacion.fechaCreacion}">
                                        <f:convertDateTime pattern="dd/MM/yyyy"/>
                                    </h:outputLabel>
                                </h:panelGrid>
                            </p:rowExpansion> -->
                        </p:dataTable>
                    </h:panelGrid>
                    </p:fieldset>
                    <!-- Fin de bloque de Oficios Firmados -->
                    <br/>
                    <br/>
                    <!-- Inicio de bloque de Historicos de Oficios Rechazados -->
                    <p:fieldset legend="Hist&oacute;rico de oficios rechazados" toggleable="true" toggleSpeed="500" collapsed="true">
                    <h:panelGrid id="panelDatosHistorico" columns="3" border="0" style="width:100% !important; height:100% !important;">
                        <p:dataTable id="tablaHistoricoOficiosRechazados" var="documentacion" value="#{documentacionOrdenMB.lstOficioHelper.listaOficiosRechazados}" editable="false" emptyMessage="#{msj['label.lista.vacia']}" paginator="true" rows="5" paginatorTemplate="Página: {CurrentPageReport} {FirstPageLink}                                     {PreviousPageLink} &amp;nbsp; Ir a página: {JumpToPageDropdown} &amp;nbsp; {NextPageLink} {LastPageLink}" currentPageReportTemplate="({currentPage} de {totalPages})" paginatorPosition="bottom">
                            <f:facet name="header">
                                #{msj[&apos;titulo.tabla.historico.oficio.rechazo&apos;]}
                            </f:facet>
                            <!-- <p:column headerText="#{msj['column.label.detalle']}" style="width:35px">
                                <p:column>
                                    <p:rowToggler/>
                                </p:column>
                            </p:column> -->
                            <p:column headerText="#{msj['column.label.id.oficio']}" style="width:7%">
                                #{documentacion.idOficio}
                            </p:column>
                            <p:column headerText="#{msj['column.label.tipo.oficio']}" style="width:15%">
                                #{documentacion.fecetTipoOficio.nombre}
                            </p:column>
