                                        <p:ajax />
                            </p:inputText></p:column>
                            </p:row>
                         	<p:row>  
	                         	<p:column></p:column>
	                         	<p:column></p:column>
	                         	<p:column></p:column>                                                                           
	                            <p:column><p:message for="txtNombreRepLegalContCompTercero"/>  </p:column>
                            </p:row>    
                            <p:row>  
	                         	<p:column><h:outputText value="#{msj['label.medio.contacto.representante.legal']}:" 
