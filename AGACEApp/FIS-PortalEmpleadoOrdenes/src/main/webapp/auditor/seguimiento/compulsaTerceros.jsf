						<p:spacer width="50px" height="20px"/>
				        <p:messages id="messagesPapelesTrabajoOficio" showDetail="true"
							closable="true" for="msgPapelesTrabajoOficio" />
				        <p:fieldset id ="fltCompulsaTerceros" widgetVar="fltCompulsaTerceros"
